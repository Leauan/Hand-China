                var field_record = records[j].getField(name);
                var ds_id = ds.id;
                var propertity_id = ds.id + '_' + body.id;
                if (!window['HY_CREATE_PROJECT_ZW1_layout_switchcard_fields_propertity'][propertity_id]) {
                    window['HY_CREATE_PROJECT_ZW1_layout_switchcard_fields_propertity'][propertity_id] = [];
                }
                if (field.isRequired()) {
                	var requiredfunction='';
                    window['HY_CREATE_PROJECT_ZW1_layout_switchcard_fields_propertity'][propertity_id].push(name);
                    field.setRequired(false);
                    field_record.setRequired(false);
                    if(field.get('requiredfunction')){
                       requiredfunction = 'HY_CREATE_PROJECT_ZW1_SWITCHCARD_REQUIREDFUNCTION';
                   	   field.setPropertity('requiredfunction', requiredfunction);
                   	   field_record.setPropertity('requiredfunction', requiredfunction);
                    }
                    if (name.substring(name.length - 2, name.length) == '_n') {
                        var value_name = name.substring(0, name.length - 2);
                        if (ds.getField(value_name)) {
                            window['HY_CREATE_PROJECT_ZW1_layout_switchcard_fields_propertity'][propertity_id].push(value_name);
                            var value_field = ds.getField(value_name);
                            var value_field_record = records[j].getField(value_name);
                            value_field.setRequired(false);
                            value_field_record.setRequired(false);
                            if(value_field.get('requiredfunction')){
	                          requiredfunction = 'HY_CREATE_PROJECT_ZW1_SWITCHCARD_REQUIREDFUNCTION';
	                       	  value_field.setPropertity('requiredfunction', requiredfunction);
	                       	  value_field_record.setPropertity('requiredfunction', requiredfunction);
	                       }
                        }
                    }
                }
                if(ds.id != switch_card_ds.id){
                  records[j].set(name, null);
                }
            }
        }
    }
    var switch_card_name = cmp.binder.name;
    var switch_card_ds = cmp.binder.ds;
    var switch_card_record = cmp.binder.ds.getCurrentRecord();
