                if (editor_record.get('allow_decimal') == 'TRUE') {
                    temp_value = value;
                } else {
                    temp_value = parseFloat(parseFloat(value).toFixed(0));
                }
            }
        }
    } else if (validation_type == 'DATEPICKER') {
        temp_value = Aurora.formatDate(value);
