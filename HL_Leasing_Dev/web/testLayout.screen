    if (Ext.isEmpty(value)) {
        var query_name = GET_CONFIG_COLUMN_NAME(name),
            field = record.getField(name),
            config_records= $('HY_CREATE_PROJECT_ZW1_RECEIPT_INFO_con_layout_config_ds').getAll();
        var current_field_value;
