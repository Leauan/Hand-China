function ON_DOC_LAYOUT_CARDHIDE(cmp, cmps, body) {
    function remove_local_record_fields(ds, grid_flag, name,switch_card_ds) {
        var records = ds.getAll();
        if (grid_flag) {
            for (var i = 0;i < records.length;) {
                ds.removeLocal(records[i]);
            }
        } else {
            for (var j = 0;j < records.length;j++) {
