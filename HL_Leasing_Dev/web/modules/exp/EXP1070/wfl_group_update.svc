<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure>
        <a:batch-apply sourcepath="/parameter">
            <p:switch test="@current_parameter/@_status">
                <p:case value="update">
                    <a:model-update model="exp.EXP1070.cfl_wfl_approver_group"/>
                    <a:model-batch-update model="exp.EXP1070.cfl_wf_approver_relation" sourcePath="@current_parameter/result_ds"/>
                </p:case>
                <p:case value="insert">
                    <a:model-insert model="exp.EXP1070.cfl_wfl_approver_group"/>
                    <a:model-batch-update model="exp.EXP1070.cfl_wf_approver_relation" sourcePath="@current_parameter/result_ds"/>
                </p:case>
                <p:case value="delete">
                    <a:model-delete model="exp.EXP1070.cfl_wfl_approver_group"/>
                    <a:model-batch-update model="exp.EXP1070.cfl_wf_approver_relation" sourcePath="@current_parameter/result_ds"/>
                </p:case>
            </p:switch>
        </a:batch-apply>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
