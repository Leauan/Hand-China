<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:task="aurora.application.task" xmlns:dr="aurora.plugin.excelreport" xmlns:mail="aurora.plugin.mail" xmlns:msg="aurora.application.features.msg" xmlns:t="uncertain.composite.transform" xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:excel="aurora.application.task.excel" trace="true">
    <a:init-procedure>
        <!--数据DS-->
        <a:model-query fetchAll="false" model="cont.CON346.con_rent_balance_query" rootPath="/model/basic_data"/>
        <dr:excel-report enableTask="false" filename="租金余额表.xls">
            <dr:styles>
                <dr:cell-style name="cell1" align="ALIGN_CENTER" borderbottom="BORDER_THIN" borderleft="BORDER_THIN" borderright="BORDER_THIN" bordertop="BORDER_THIN" vertical="VERTICAL_CENTER">
                    <dr:font bold="false" fontName="宋体" height="12"/>
                </dr:cell-style>
                <dr:cell-style name="cell2" align="ALIGN_RIGHT" borderbottom="BORDER_THIN" borderleft="BORDER_MEDIUM" borderright="BORDER_MEDIUM" bordertop="BORDER_THIN" vertical="VERTICAL_CENTER">
                    <dr:font bold="false" fontName="宋体" height="12"/>
                </dr:cell-style>
                <dr:cell-style name="cell3" align="ALIGN_LEFT" borderbottom="BORDER_THIN" borderleft="BORDER_MEDIUM" borderright="BORDER_THIN" bordertop="BORDER_THIN" vertical="VERTICAL_CENTER">
                    <dr:font bold="false" fontName="宋体" height="12"/>
                </dr:cell-style>
                <dr:cell-style name="cell4" align="ALIGN_CENTER" borderbottom="BORDER_THIN" borderleft="BORDER_THIN" borderright="BORDER_MEDIUM" bordertop="BORDER_THIN" vertical="VERTICAL_CENTER">
                    <dr:font bold="false" fontName="宋体" height="12"/>
                </dr:cell-style>
                <dr:cell-style name="header" align="ALIGN_LEFT" borderbottom="BORDER_THIN" borderright="BORDER_THIN" bordertop="BORDER_THIN" cell_style_id="2" foregroundcolor="BLACK">
                    <dr:font bold="true" cell_style_id="2" fontname="宋体" height="10"/>
                </dr:cell-style>
            </dr:styles>
            <dr:sheets>
                <dr:sheet name="租金余额报表信息" displayGridlines="false">
                    <dr:dynamic-content cell="A" dataModel="/model/basic_data" row="1">
                        <dr:columns>
                            <dr:table-column cellStyle="cell1" field="agent_code" title="经销商编码" type="content"/>
                            <dr:table-column cellStyle="cell2" field="agent_name" title="经销商名称" type="content"/>
                            <dr:table-column cellStyle="cell1" field="tenant_code" title="承租人编码" type="content"/>
                            <dr:table-column cellStyle="cell1" field="tenant_name" title="承租人名称" type="content"/>
                            <dr:table-column cellStyle="cell1" field="liv_province" title="省份" type="content"/>
                            <dr:table-column cellStyle="cell1" field="contract_number" title="合同编码" type="content"/>
                            <dr:table-column cellStyle="cell1" field="inception_of_lease" title="起租日" type="content"/>
                            <dr:table-column cellStyle="cell1" field="product_name_write" title="产品名称" type="content"/>
                            <dr:table-column cellStyle="cell1" field="series_id_n" title="车型" type="content"/>
                            <dr:table-column cellStyle="cell1" field="int_rate_display" title="利率" type="content"/>
                            <dr:table-column cellStyle="cell1" field="base_rate" title="基准利率" type="content"/>
                            <dr:table-column cellStyle="cell3" field="lease_times" title="租赁期间" type="content"/>
                            <dr:table-column cellStyle="cell1" field="down_payment_ratio" title="首付比例" type="content"/>
                            <dr:table-column cellStyle="cell1" field="invoice_price" title="购置价" type="content"/>
                            <dr:table-column cellStyle="cell1" field="residual_value" title="留购价" type="content"/>
                            <dr:table-column cellStyle="cell1" field="total_rental" title="租金" type="content"/>
                            <dr:table-column cellStyle="cell1" field="down_payment" title="首付款" type="content"/>
                            <dr:table-column cellStyle="cell1" field="current_due_date" title="当前收款预定日" type="content"/>
                            <dr:table-column cellStyle="cell1" field="current_due_amount" title="当前应收款金额" type="content"/>
                            <dr:table-column cellStyle="cell4" field="current_unreceived_amount" title="当前已收款金额" type="content"/>
                            <dr:table-column cellStyle="cell1" field="current_unreceived_amount" title="当前未收款金额" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_due_amount" title="当月末应收款余额合计" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_principal_amount" title="当月末本金余额合计" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_interest_amount" title="当月末利息余额合计" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_vat_interest_amount" title="当月末利息余额增值税" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_residual_amount" title="当月末留购金余额" type="content"/>
                            <dr:table-column cellStyle="cell1" field="overdue_times" title="逾期期数" type="content"/>
                            <dr:table-column cellStyle="cell1" field="overdue_days" title="逾期天数" type="content"/>
                            <dr:table-column cellStyle="cell1" field="month_overdue_amount" title="当前逾期金额合计" type="content"/>
                        </dr:columns>
                    </dr:dynamic-content>
                </dr:sheet>
            </dr:sheets>
        </dr:excel-report>
    </a:init-procedure>
</a:service>
