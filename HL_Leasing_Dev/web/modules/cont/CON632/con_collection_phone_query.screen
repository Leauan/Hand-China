<a:screen xmlns:a="http://www.aurora-framework.org/application" customizationEnabled="true">
    <a:view>
        <script><![CDATA[
            /*  Aurora.onReady(function() {
             alert('${/parameter/@type_desc}');
             });
             */
        ]]></script>
        <a:dataSets>
            <a:dataSet id="${/parameter/@type}_con_collection_call_phone_result_ds" autoQuery="true" model="cont.CON632.con_collection_call_ln" queryUrl="${/request/@context_path}/autocrud/cont.CON632.con_collection_call_ln/query?type=${/parameter/@type}&amp;contract_id=${/parameter/@contract_id}" selectable="true" selectionModel="single"/>
        </a:dataSets>
        <a:grid id="${/parameter/@type}_grid_id" bindTarget="${/parameter/@type}_con_collection_call_phone_result_ds" marginHeight="300" marginWidth="75" navBar="true" title="历史电催记录">
            <a:columns>
                <a:column name="bp_type_desc" align="center" prompt="类型" width="120"/>
                <a:column name="collection_name" align="center" prompt="催收对象" width="120"/>
                <a:column name="cell_phone" align="center" prompt="电话" width="120"/>
                <a:column name="note" align="center" editor="${/parameter/@type}_note_ta" prompt="电催记录" width="120"/>
                <a:column name="call_time" align="center" prompt="登记时间" width="120"/>
                <a:column name="staff_name" align="center" prompt="电催人员" width="120"/>
            </a:columns>
            <a:editors>
                <a:textArea id="${/parameter/@type}_note_ta" editable="false"/>
            </a:editors>
        </a:grid>
    </a:view>
</a:screen>
