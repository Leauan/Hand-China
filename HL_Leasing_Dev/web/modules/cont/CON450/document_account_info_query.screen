<a:screen xmlns:a="http://www.aurora-framework.org/application" customizationEnabled="true" trace="true">
    <a:view>
        <script><![CDATA[
            function con_contract_info_query(){
                $('document_account_ds').query();
            }

        ]]></script>
        <a:dataSets>
            <a:dataSet id="for_query_ds" autoCreate="true">
                <a:fields>
                    <a:field name="start_date"/>
                    <a:field name="end_date"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="document_account_ds" autoPageSize="true" autoQuery="true" model="cont.CON450.document_account_info" queryDataSet="for_query_ds"/>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:gridButton id="hls_standard_his_id" click="con_contract_info_query" text="查询"/>
            </a:screenTopToolbar>
            <a:form column="4" labelWidth="150" marginWidth="80" title="查询条件">
                <a:datePicker name="start_date" bindTarget="for_query_ds" prompt="创建时间从"/>
                <a:datePicker name="end_date" bindTarget="for_query_ds" prompt="创建时间到"/>
            </a:form>
            <a:grid bindTarget="document_account_ds" marginHeight="180" marginWidth="80" navBar="true">
                <a:toolBar>
                    <a:button type="excel"/>
                </a:toolBar>
                <a:columns>
                    <a:column name="project_number" align="right" prompt="创建总量"/>
                    <a:column name="no_submit_number" align="right" prompt="未提交"/>
                    <a:column name="approving_number" align="right" prompt="租赁申请中"/>
                    <a:column name="close_number" align="right" prompt="取消"/>
                    <a:column prompt="风控审批">
                        <a:column name="return_approved_number" align="right" prompt="二次审批"/>
                        <a:column name="approved_number" align="right" prompt="一次审批"/>
                    </a:column>
                    <a:column prompt="风控审批结果">
                        <a:column name="no_conditional_number" align="right" prompt="直接通过"/>
                        <a:column name="conditional_number" align="right" prompt="附条件通过"/>
                        <a:column name="reject_number" align="right" prompt="拒绝"/>
                    </a:column>
                    <a:column prompt="合同创建">
                        <a:column name="contract_create_number" align="right" prompt="已创建"/>
                        <a:column name="contract_cancel_number" align="right" prompt="合同取消"/>
                    </a:column>
                    <a:column prompt="合同签约">
                        <a:column name="contract_sign_number" align="right" prompt="已签"/>
                        <a:column name="contract_unsign_number" align="right" prompt="未签"/>
                    </a:column>
                    <a:column prompt="合同起租">
                        <a:column name="contract_incept_number" align="right" prompt="已起租"/>
                        <a:column name="contract_no_incept_number" align="right" prompt="未起租"/>
                    </a:column>
                    <a:column prompt="GPS">
                        <a:column name="contract_gps_number" align="right" prompt="已装"/>
                        <a:column name="contract_no_gps_number" align="right" prompt="未装"/>
                    </a:column>
                    <a:column name="contract_licence_number" align="right" prompt="上牌"/>
                    <a:column name="contract_insurance_number" align="right" prompt="保险"/>
                    <a:column name="contract_get_car_number" align="right" prompt="提车"/>
                </a:columns>
            </a:grid>
        </a:screenBody>
    </a:view>
</a:screen>
