<a:service xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure>
        <s:server-script><![CDATA[
            importPackage(java.io);
            importPackage(com.test);
            try {
                var xml_temp;
                var deal_result;
                var parameter = $ctx.parameter;
                var manual_pub = $bm('db.hls_ebank_cup_pub_pkg.hl_cup_sign_request');
                var manual_pub_result = manual_pub.execute({
                    "dd_bank_account_num": parameter.dd_bank_account_num,
                    "dd_bank_account_name": parameter.dd_bank_account_name,
                    "id_type": parameter.id_type,
                    "id_number": parameter.id_number,
					"direct_debit_bank_id":parameter.direct_debit_bank_id,
                    "bank_reserved_phone": parameter.bank_reserved_phone,
					"contract_number":parameter.contract_number,
                    "xml_temp": xml_temp
                });
                var hlCupSign = new HlCupSign();
                var result = hlCupSign.TestPayReq(parameter.xml_temp);
            
                var deal_transaction = $bm('db.hls_ebank_cup_pub_pkg.deal_hl_cup_sign_request');
                deal_transaction.execute({
                    "xml_clob": result,
                    "deal_result": deal_result
                });
				 println("<1>" + 1);
				 println("<result>" + result);
                println("<deal_result>" + parameter.deal_result);
            } catch (e) {
                println("错误信息" + e);
            }
        ]]></s:server-script>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
