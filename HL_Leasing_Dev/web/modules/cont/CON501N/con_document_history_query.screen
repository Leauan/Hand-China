<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure><![CDATA[
		
	]]></a:init-procedure>
    <a:view>
        <script><![CDATA[
			function con333_back() {
			    $('contract_history_window').close();
			}
			
		]]></script>
        <a:dataSets>
            <a:dataSet id="con_document_history_result_ds" autoPageSize="true" autoQuery="true" model="cont.CON333.yonda_doc_status_history_vl" queryUrl="${/request/@context_path}/autocrud/cont.CON333.yonda_doc_status_history_vl/query?contract_id=${/parameter/@contract_id}">
                <a:fields>
                    <a:field name="comment_text" readOnly="true"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:gridButton click="con333_back" text="后退"/>
            </a:screenTopToolbar>
        </a:screenBody>
        <a:grid id="con_document_history_result_grid" bindTarget="con_document_history_result_ds" marginHeight="200" marginWidth="30" navBar="true" showRowNumber="true">
            <a:columns>
                <a:column name="document_category" align="center" prompt="单据类型" width="120"/>
                <a:column name="document_number" align="center" prompt="单据编号" width="120"/>
                <a:column name="description" align="center" prompt="操作人员" width="120"/>
                <a:column name="position_name" align="center" prompt="操作人员岗位" width="120"/>
                <a:column name="status_desc" align="center" prompt="操作明细" width="120"/>
                <a:column name="operation_time" align="center" prompt="操作时间" renderer="Aurora.formatDate" width="120"/>
                <a:column name="action_type_desc" align="center" prompt="工作流审批工作" width="120"/>
                <a:column name="comment_text" align="center" editor="textArea_id" prompt="工作流审批信息" width="250"/>
                <a:column name="current_status_flag_des" align="center" prompt="是否单据最后状态"/>
            </a:columns>
            <a:editors>
                <a:textArea id="textArea_id" height="120"/>
            </a:editors>
        </a:grid>
    </a:view>
</a:screen>
