<a:screen xmlns:c="aurora.application.action" xmlns:s="aurora.plugin.script" xmlns:w="aurora.plugin.export.word" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" customizationEnabled="true" trace="true">
    <a:init-procedure>
        <a:model-query defaultWhereClause="t1.contract_id=${/parameter/@contract_id}" fetchAll="true" model="cont.CON510.con_contract_v_for_print" rootPath="con_contract_info_path"/>
        <a:model-query fetchAll="true" model="cont.CON510.con_contract_cashflow" rootPath="con_contract_cashflow_path"/>
        <w:word-export name="${/parameter/@content_number}.pdf" savePath="${/parameter/@doc_name}" template="aurora.plugin.export.word/${/parameter/@templt_name}" type="pdf"><![CDATA[
        ]]></w:word-export>
        <s:server-script import="seal_util.js"/>
    </a:init-procedure>
</a:screen>
