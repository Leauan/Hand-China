<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:s="aurora.plugin.script" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="false">
    <a:init-procedure>
        <a:model-update model="csh.CSH602.yonda_hls_tl_check_table_authority"/>
        <s:server-script import="yonda_itfc_config/tl_withholding_config.js"><![CDATA[
            importPackage(Packages.com.allinpay.xmltrans.yondatl);
            var yondatl_itfc = new YondaTl($instance('aurora.database.service.IDatabaseServiceFactory'), $instance('uncertain.ocm.IObjectRegistry'), $ctx.getData());
            var batch_daishou_bm = $bm('csh.CSH602.yonda_hls_tl_batch_daishou');
            var merchantid = tl_withholding_config['merchantId'];
            $ctx.parameter.merchantid = merchantid;
            batch_daishou_bm.update($ctx.parameter);
            if ($ctx.parameter.tl_itfc_header_id) {
                yondatl_itfc.run($ctx.parameter.tl_itfc_header_id, '100001');
            }
        ]]></s:server-script>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
