<a:screen xmlns:a="http://www.aurora-framework.org/application" customizationEnabled="true" trace="true">
    <a:init-procedure/>
    <a:view>
        <a:link id="prj_chance_con_contract_update_link_id" url="${/request/@context_path}/modules/cont/CON500/con_contract_update.screen"/>
        <a:link id="con_contract_get_layout_code_link_id" model="cont.CON500.con_contract_get_layout_code" modelaction="update"/>
        <a:link id="sys_service_welcome_link" url="${/request/@context_path}/welcome.screen"/>
        <script><![CDATA[
            function on_prj_chance_create_entrance_sure() {
                if($('prj_document_create_entrance_ds').validate()){
	                var record = $('prj_document_create_entrance_ds').getAt(0);
	                record.set('function_code','PRJ400');
	                record.set('function_usage', 'CREATE');
	                record.set('document_category', 'CHANCE');
	                record.set('default_value_dsid', 'prj_document_create_entrance_ds');
	                record.data['url_title']='${l:PRJ400.PRJ_CHANCE_CREATE_ENTRANCE.CREATE_CHANCE}';
	                var param = record.data;
	                hls_doc_get_layout_code('con_contract_get_layout_code_link_id',param,'prj_chance_con_contract_update_link_id');
                }
            }
            
            function on_prj_chance_create_entrance_cancle() {
                location.href = $('sys_service_welcome_link').getUrl();
                parent.shMenu('show');
            }
        ]]></script>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:screenTitle/>
                <a:gridButton click="on_prj_chance_create_entrance_sure" text="HLS.CONFIRM"/>
                <a:gridButton click="on_prj_chance_create_entrance_cancle" text="HLS.EXIT"/>
            </a:screenTopToolbar>
            <a:screen-include screen="modules/cont/CON500/con_contract_get_layout_code.screen"/>
            <a:screen-include screen="modules/prj/PRJ400/prj_document_create_entrance.screen?document_category=CHANCE"/>
        </a:screenBody>
    </a:view>
</a:screen>
