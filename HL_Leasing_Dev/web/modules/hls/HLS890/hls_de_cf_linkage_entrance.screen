        <script><![CDATA[
            function open_agent_bp_win(ds_id,record_id) {
                var record = $(ds_id).findById(record_id);
                var param = record.data;
                param['bp_id'] = record.get('bp_id');
                param['function_code'] = 'HLS890D';
                param['function_usage'] = 'UPDATE';
                param['bp_class'] = record.get('bp_class');
                param['maintain_type'] = 'UPDATE';
                param['url_title'] = '联动利率维护';
                hls_doc_get_layout_code('con_contract_get_layout_code_link_id', param, 'car_modify_special_link', ds_id, '${/parameter/@layout_code}');
            }
            function open_linkage_detail_win(ds_id,record_id) {
                var record = $(ds_id).findById(record_id);
                var param = record.data;
                param['bp_id'] = record.get('bp_id');
                param['function_code'] = 'HLS890R';
                param['function_usage'] = 'UPDATE';
                param['bp_class'] = record.get('bp_class');
                param['maintain_type'] = 'UPDATE';
                param['url_title'] = '联动政策定义';
                hls_doc_get_layout_code('con_contract_get_layout_code_link_id', param, '${/parameter/@layout_code}_hls_linkage_modify_hd_link_id');
            }
            window['${/parameter/@layout_code}_dynamic_link_renderer'] = function(value, record, name, config_record) {
                if (name == 'bp_code' && value) {
                    return '<a href="javascript:open_agent_bp_win(\'' + record.ds.id + '\',\'' + record.id + '\')">' + value + '</a>';
                }else if (name=='linkage_detail'){
                     return '<a href="javascript:open_linkage_detail_win(\'' + record.ds.id + '\',\'' + record.id + '\')">' + '联动政策定义' + '</a>';
                }
                return value;
            };
            // window['${/parameter/@layout_code}_on_layout_dynamic_grid_query'] = function(ds, qpara, bp_seq) {
