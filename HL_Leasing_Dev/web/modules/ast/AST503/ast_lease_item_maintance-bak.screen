        <script><![CDATA[
            var g_item_detail_id = -1;
            
            
            function renewal_application() {
                var record = $('ast_car_insurance_result_ds').getSelected()[0];
                if (Aurora.isEmpty(record)) {
                    Aurora.showMessage('${l:PROMPT}', '请选择一行数据');
                    Aurora.Masker.unmask(Ext.getBody());
                    return;
                }
            
                if (record.isNew) {
                    Aurora.showMessage('提示', '请先保存');
                    return;
                }
                Aurora.showConfirm('确认', '您确定续保吗？', function() {
                    Aurora.Masker.mask(Ext.getBody(), '正在提交......');
                    var contract_id = $('con_item_info_ds').getSelected()[0].get('contract_id');
                    var car_insurance_id = record.get('ast_car_insurance_id');
                    var document_status = record.get('document_status');
                    if (document_status != '10') {
                        Aurora.showMessage('${l:PROMPT}', '单据状态必须为新建');
                        Aurora.Masker.unmask(Ext.getBody());
                        return;
                    }
                    var compulsory_insurance_amount = record.get('compulsory_insurance_amount');
                    var damage_insurance_amount = record.get('damage_insurance_amount');
                    var third_party_insurance_amount = record.get('third_party_insurance_amount');
                    var basic_non_deductible_amount = record.get('basic_non_deductible_amount');
                    var pilfer_insurance_amount = record.get('pilfer_insurance_amount');
            
                    var commercial_insurance_amount = record.get('commercial_insurance_amount');
                    var glass_insurance_amount = record.get('glass_insurance_amount');
                    var seat_insurance_amount = record.get('seat_insurance_amount');
                    var combustion_insurance_amount = record.get('combustion_insurance_amount');
            
                    if (Aurora.isEmpty(compulsory_insurance_amount)) {
                        compulsory_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(damage_insurance_amount)) {
                        damage_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(third_party_insurance_amount)) {
                        third_party_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(basic_non_deductible_amount)) {
                        basic_non_deductible_amount = 0;
                    }
                    if (Aurora.isEmpty(pilfer_insurance_amount)) {
                        pilfer_insurance_amount = 0;
                    }
            
                    if (Aurora.isEmpty(commercial_insurance_amount)) {
                        commercial_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(glass_insurance_amount)) {
                        glass_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(seat_insurance_amount)) {
                        seat_insurance_amount = 0;
                    }
                    if (Aurora.isEmpty(combustion_insurance_amount)) {
                        combustion_insurance_amount = 0;
                    }
            
                    var due_amount = compulsory_insurance_amount + damage_insurance_amount + third_party_insurance_amount + basic_non_deductible_amount + pilfer_insurance_amount + commercial_insurance_amount + glass_insurance_amount + seat_insurance_amount + combustion_insurance_amount;
                    //必须上传保险附件
                    Aurora.request({
                        url: $('get_atm_id_exist').getUrl(),
                        para: {
                            table_name: 'AST_CAR_INSURANCE',
                            table_pk_value: car_insurance_id
            
                        },
                        success: function(res) {
                            var count = res.result.record['atm_num'];
                            if (count > 0) {
                                Aurora.request({
                                    url: $('ast503_renewal_link').getUrl(),
                                    para: {
                                        contract_id: contract_id,
                                        due_amount: due_amount,
                                        car_insurance_id: car_insurance_id
                                    },
                                    success: function(res) {
                                        $('ast_car_insurance_result_ds').query();
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    failure: function() {
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    error: function() {
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    scope: this,
                                    sync: true
                                });
                            } else {
                                Aurora.Masker.unmask(Ext.getBody());
                                Aurora.showMessage('提示', '请先上传保险相关附件');
                            }
                        },
                        failure: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        },
                        error: function() {
                            Aurora.Masker.unmask(Ext.getBody());
                        }
                    });
            
            
                });
            
            }
            
            function pay_claim_expense() {
                Aurora.Masker.mask(Ext.getBody(), '正在提交');
                var contract_id = $('con_item_info_ds').getSelected()[0].get('contract_id');
                var records = $('ast_car_insurance_records_ds').getSelected();
                if (records.length != 1) {
                    Aurora.showMessage('${l:PROMPT}', '请选择一行数据');
                    Aurora.Masker.unmask(Ext.getBody());
                    return;
                }
                var document_status = records[0].get('document_status');
                if (document_status != '10') {
                    Aurora.showMessage('${l:PROMPT}', '单据状态必须为新建');
                    Aurora.Masker.unmask(Ext.getBody());
                    return;
                }
                var car_insurance_record_id = records[0].get('ast_car_insurance_records_id');
                var due_amount = records[0].get('settle_claim_amount');
                if (Aurora.isEmpty(car_insurance_record_id) || records[0].dirty == true) {
                    Aurora.showMessage('${l:PROMPT}', '请先保存!');
                    Aurora.Masker.unmask(Ext.getBody());
                    return;
                }
                if (Aurora.isEmpty(due_amount)) {
                    Aurora.showMessage('${l:PROMPT}', '请先维护出险金额!');
                    Aurora.Masker.unmask(Ext.getBody());
                    return;
                }
                Aurora.showConfirm('确认', '您确定支付理赔款？', function() {
                    //必须上传保险附件
                    Aurora.request({
                        url: $('get_atm_id_exist').getUrl(),
                        para: {
                            table_name: 'AST_CAR_INSURANCE_RECORDS',
                            table_pk_value: car_insurance_record_id
            
                        },
                        success: function(res) {
                            var count = res.result.record['atm_num'];
                            if (count > 0) {
                                Aurora.request({
                                    url: $('ast503_pay_claim_expense').getUrl(),
                                    para: {
                                        contract_id: contract_id,
                                        due_amount: due_amount,
                                        car_insurance_record_id: car_insurance_record_id
                                    },
                                    success: function(res) {
                                        $('ast_car_insurance_records_ds').query();
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    failure: function() {
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    error: function() {
                                        Aurora.Masker.unmask(Ext.getBody());
                                    },
                                    scope: this,
                                    sync: true
                                });
                            } else {
                                Aurora.Masker.unmask(Ext.getBody());
                                Aurora.showMessage('提示', '请先上传理赔相关附件');
                            }
                        }
                    });
            
                });
                Aurora.Masker.unmask(Ext.getBody());
            }
            
