<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure/>
    <a:view>
        <script><![CDATA[
		]]></script>
        <a:dataSets><![CDATA[
            
        ]]></a:dataSets>
        <a:grid id="ast_car_gps_grid_id" bindTarget="ast_car_gps_result_ds" height="200" marginWidth="30" navBar="true">
            <a:columns>
                <a:column name="imei" prompt="IMEI" renderer="Aurora.formatDate" width="120"/>
                <a:column name="install_date" prompt="安装时间" renderer="Aurora.formatDate" width="100"/>
                <a:column name="gps_provider" prompt="GPS供应商" width="120"/>
                <a:column name="procurement_price" align="right" prompt="采购价格" renderer="Aurora.formatMoney" width="120"/>
                <a:column name="install_fee" align="right" prompt="安装杂费" renderer="Aurora.formatMoney" width="120"/>
                <a:column name="gps_fee" align="right" prompt="GPS年费" renderer="Aurora.formatMoney" width="120"/>
                <a:column name="remove_date" prompt="拆除时间" renderer="Aurora.formatDate" width="120"/>
                <a:column name="gps_fee_back" align="right" prompt="GPS费用退还" renderer="Aurora.formatMoney" width="120"/>
                <a:column name="description" prompt="备注" width="180"/>
                <a:column name="enabled_flag" prompt="启用" width="50"/>
            </a:columns>
            <a:editors>
                <a:numberField id="ast_car_gps_nf"/>
                <a:textField id="ast_car_gps_tf"/>
                <a:datePicker id="ast_car_gps_dp"/>
                <a:checkBox id="ast_car_gps_cb"/>
            </a:editors>
        </a:grid>
        <a:hBox/>
    </a:view>
</a:screen>
