<?xml version="1.0" encoding="UTF-8"?>
<ns1:model xmlns:ns2="aurora.database.local.oracle" xmlns:ns1="http://www.aurora-framework.org/schema/bm">
    <ns1:operations>
        <ns1:operation name="query">
            <ns1:query-sql><![CDATA[
				SELECT code_value,
				       code_value_name
				  FROM sys_code_values_v c
				 WHERE c.code = 'HLS_ACR_PAYMENT_METHOD'
				 AND c.code_value IN ('04','05')
            ]]></ns1:query-sql>
        </ns1:operation>
    </ns1:operations>
</ns1:model>
