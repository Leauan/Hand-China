<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select 
  t.item_id,
  t.post_id,
  (t.item_type||t.sex_type||t.birtth_date||t.marital_status||t.academic_background||
  t.academic_degree||t.phone||t.cell_phone||t.work_phone||t.mail||
  t.address||t.zip_code||t.card_address||t.name_sp||t.card_type_sp||
  t.id_card_no_sp||t.address_sp||t.cell_phone_sp||t.job_item_type||t.job_code||
  t.unit_name||t.industry||t.unit_address||t.unit_zipcode||t.unit_year||t.job_level||
  t.job_name||t.income||t.paycard_num||t.paycard_bank||t.live_item_type||t.live_address||
  t.live_zipcode||t.live_condition) identify_str
from hls_zx_post_identity t
where t.item_id = ${@item_id}]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="item_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="item_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="ITEM_ID"/>
        <bm:field name="post_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="POST_ID"/>
        <bm:field name="identify_str" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="IDENTIFY_STR"/>
    </bm:fields>
</bm:model>
