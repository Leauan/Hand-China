<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select 
     l.post_id,
     (l.zx_version||l.zx_org_code||l.date_str||l.base_version||l.resend_flag||
     l.post_type||l.record_num_str||l.start_date||l.end_date||l.contact_man_str||l.contact_phone_str||
     l.ref_v01) head_str,
     l.file_name,
     l.file_path
   from hls_zx_post_lv l
   where l.post_id = ${@post_id}]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="post_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="post_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="POST_ID"/>
        <bm:field name="head_str" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="HEAD_STR"/>
        <bm:field name="file_path"/>
        <bm:field name="file_name"/>
    </bm:fields>
</bm:model>
