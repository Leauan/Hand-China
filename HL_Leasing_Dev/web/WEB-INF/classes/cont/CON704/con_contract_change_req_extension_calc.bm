<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin
            		con_change_req_extension_pkg.change_req_calc(
            			p_change_req_id    =>${@change_req_id},
                     	p_calc_session_id  =>${@calc_session_id},
                     	p_calc_successful  =>${@calc_successful},
                 		p_warning_message  =>${@warning_message},
                     	p_user_id          =>${/session/@user_id});
            	end;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="calc_session_id" dataType="java.lang.String" input="true" output="true" outputPath="@calc_session_id"/>
                <bm:parameter name="calc_successful" dataType="java.lang.String" input="false" output="true" outputPath="@calc_successful"/>
                <bm:parameter name="warning_message" dataType="java.lang.String" input="false" output="true" outputPath="@warning_message"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
