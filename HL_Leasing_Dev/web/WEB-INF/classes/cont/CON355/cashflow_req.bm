<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_cashflow_pkg.delete_cashflow(p_contract_id =>${@contract_id},p_cashflow_id =>${@cashflow_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_cashflow_pkg.insert_cashflow(p_contract_id =>${@contract_id},p_cashflow_id =>${@cashflow_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
