<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select et_agreement_id,document_type from con_contract_et_hd where et_agreement_id = ${/parameter/@et_agreement_id}]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter inputPath="/parameter/@et_agreement_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="et_agreement_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="et_agreement_id" required="true"/>
        <bm:field name="document_type" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="document_type"/>
    </bm:fields>
</bm:model>
