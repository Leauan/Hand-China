<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                UPDATE
                    con_contract t
                SET
                    t.con_content_confirm_status='Y'
                WHERE
                    t.contract_id=${@contract_id}
                    and t.data_class='NORMAL'
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="contract_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
