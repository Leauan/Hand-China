<bm:model xmlns:ns2="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
           select count(1) new_apply_num from con_contract_add_apply t 
           where   t.contract_id=${@contract_id} and  t.apply_status='NEW'
        ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
