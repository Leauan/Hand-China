<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_redemption_certificate_pkg.cancel_con_redemption(p_batch_id =>${@batch_id} , 
                                                      p_user_id =>${/session/@user_id} 
                                                      );
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_redemption_certificate_pkg.con_redemption_wfl_start(p_batch_id =>${@batch_id} , 
                                                      p_user_id =>${/session/@user_id} 
                                                      );
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
