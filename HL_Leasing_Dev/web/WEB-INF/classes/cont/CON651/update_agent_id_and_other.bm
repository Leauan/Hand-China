<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_df_car_confirm_pkg.update_agent_id_and_other(p_ln_id    =>  ${@line_id} ,
                                                      p_after_id                =>  ${@after_id} ,
                                                      p_move_date               =>  to_date(${@move_date}, 'yyyy-mm-dd'),
                                                      p_remove_date             =>  to_date(${@remove_date}, 'yyyy-mm-dd'),
                                                      p_phone                   =>  ${@phone},
                                                      p_contact_person          =>  ${@contact_person},
                                                      p_user_id                 =>  ${/session/@user_id}
                                                      );
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_df_car_confirm_pkg.cancel_apply_for_move(p_hd_id    =>  ${@hd_id} ,
                                                      p_user_id             =>  ${/session/@user_id}
                                                      );
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_df_car_confirm_pkg.update_move_cars_for_wfl(p_ln_id        =>   ${@line_id} ,
                                                                    p_deposit_flag =>   ${@deposit_flag},
                                                                    p_user_id      =>   ${/session/@user_id}
                                                                    );
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
