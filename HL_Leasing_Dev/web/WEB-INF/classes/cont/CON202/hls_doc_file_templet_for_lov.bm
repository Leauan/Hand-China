<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="hls_doc_file_templet" needAccessControl="false">
    <bm:fields>
        <bm:field name="templet_id" databaseType="NUMBER" datatype="java.lang.Long" forDisplay="false" forQuery="false"/>
        <bm:field name="templet_code" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" prompt="模板代码"/>
        <bm:field name="templet_name" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" prompt="模板名称"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="templet_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field name="templet_code" queryExpression="t1.templet_code like ${@templet_code}"/>
        <bm:query-field name="templet_name" queryExpression="t1.templet_name like ${@templet_name}"/>
    </bm:query-fields>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="t1.enabled_flag=&apos;Y&apos;"/>
    </bm:data-filters>
</bm:model>
