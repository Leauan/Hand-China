<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_collection_tool_pkg.insert_collection_tool_data( p_contract_id => ${@contract_id}, 
                    													p_project_id=> ${@project_id}, 
                    													p_collection_measures => ${@collection_measures}, 
                    													p_province => ${@province}, p_city => ${@city}, 
                    													p_visit_address => ${@visit_address}, 
                    													p_register_record => ${@register_record}, 
                    													p_detailed_address => ${@detailed_address},
                    													p_province_b       =>${@province_b},
                                        								p_city_b              =>${@city_b},
                                        								p_visit_address_b     =>${@visit_address_b},
                                        								p_detailed_address_b =>${@detailed_address_b}, 
                    													p_user_id => ${/session/@user_id}, 
                    													p_staff_id =>${@staff_id},
                    													p_collection_id=>${@collection_id},
                    													p_company_id=>${/session/@company_id},
                    													p_col_id=>${@col_id},
                    													p_collection_cost=>${@collection_cost},
                    													p_lawsuit_reason =>${@lawsuit_reason},
                    													p_t_take_reason  =>${@take_reason},
                                        								p_t_take_imformation_add => ${@take_imformation_add});
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="collection_id" output="true" outputPath="@collection_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
        		begin
  				-- Call the procedure
  				con_collecton_wfl_pkg.con_collection_submit(p_collection_id => ${@collection_id},
                                              				p_user_id => ${/session/@user_id});
				end;
        	
        	]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
