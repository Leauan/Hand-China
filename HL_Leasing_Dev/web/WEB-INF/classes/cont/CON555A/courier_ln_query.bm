<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    CON_CONTRACT_LV ccl
                WHERE
                    ccl.contract_id IN
                    (SELECT
                        cel.contract_id
                    FROM
                        CON_EXPRESS_LN cel
                    WHERE
                        cel.express_id=${/parameter/@express_id}
                    )
            ]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter inputPath="/parameter/@express_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
