<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:fields>
        <bm:field name="project_id" forInsert="true" physicalName="PROJECT_ID" prompt="单据ID"/>
        <bm:field name="project_number" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" forSelect="true" physicalName="DOCUMENT_NUMBER" prompt="单据编号"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select t1.project_id,project_number from prj_project t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="project_number" queryExpression="t1.project_number like ${@project_number}"/>
    </bm:query-fields>
</bm:model>
