<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="AST_CAR_GPS_ADDRESS" defaultOrderBy="t1.gps_address_id desc">
    <bm:fields>
        <bm:field name="gps_address_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="GPS_ADDRESS_ID" prompt="AST_CAR_GPS_ADDRESS.GPS_ADDRESS_ID"/>
        <bm:field name="ast_car_gps_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="AST_CAR_GPS_ID" prompt="AST_CAR_GPS_ADDRESS.AST_CAR_GPS_ID"/>
        <bm:field name="address" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ADDRESS" prompt="AST_CAR_GPS_ADDRESS.ADDRESS"/>
        <bm:field name="creation_date_dis" expression="to_char(t1.creation_date,&apos;yyyy-mm-dd hh24:mi:ss&apos;)" forInsert="false" forUpdate="false"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:data-filters>
        <bm:data-filter name="ast_car_gps_id" enforceOperations="query" expression="exists(select 1 from ast_car_gps acg where acg.ast_car_gps_id = t1.ast_car_gps_id and acg.contract_id=${/parameter/@contract_id})"/>
    </bm:data-filters>
</bm:model>
