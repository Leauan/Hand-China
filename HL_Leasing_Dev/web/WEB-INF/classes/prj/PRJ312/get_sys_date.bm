<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    (SELECT TO_CHAR(sysdate, 'yyyy') FROM dual
                    ) YEAR,
                    (SELECT TO_CHAR(sysdate, 'mm') FROM dual
                    ) MONTH,
                    (SELECT TO_CHAR(sysdate, 'dd') FROM dual
                    ) DAY
                FROM
                    dual
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
