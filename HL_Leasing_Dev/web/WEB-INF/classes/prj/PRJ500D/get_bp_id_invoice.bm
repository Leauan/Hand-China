<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select * from (select a.unit_id,b.bp_id,b.bp_name from exp_org_unit a,hls_bp_master b where 
a.bp_id_invoice=b.bp_id) t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="unit_id" queryExpression="t1.unit_id=${@unit_id}"/>
    </bm:query-fields>
</bm:model>
