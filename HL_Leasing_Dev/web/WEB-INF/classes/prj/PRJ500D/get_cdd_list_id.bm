<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    p.cdd_list_id,
                    li.product_plan_id
                FROM
                    prj_project p,
                    prj_project_lease_item li
                WHERE
                    li.project_id=p.project_id AND
                    p.project_id = ${@project_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
