<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_contract_pkg.get_purchase_tax_flag(p_project_id => ${@project_id}, p_purchase_tax_flag => ${@purchase_tax_flag});
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="purchase_tax_flag" dataType="java.lang.String" input="false" inputPath="@purchase_tax_flag" output="true" outputPath="@purchase_tax_flag"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
