<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    prj_project_pkg.check_price_list( p_project_id         =>${@project_id},
                                                      p_price_list         =>${@price_list},
                       								  p_lease_times        =>${@lease_times},
                                                      p_balloon_ratio      =>${@balloon_ratio},
                                                      p_deposit_ratio      =>${@deposit_ratio},
                            						  p_down_payment_ratio =>${@down_payment_ratio},
                            						  p_lease_charge_ratio =>${@lease_charge_ratio},
                            						  p_customer_ratio     =>${@customer_ratio},
                                                      p_user_id =>${/session/@user_id} ) ;
                end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
