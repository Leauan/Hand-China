<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[  select * from  (select t1.unit_id,t2.organization_code,t2.organization_name,t2.organization_id from exp_org_unit t1,hls_car_organization t2 where t1.yonda_organization_code=t2.organization_code) t
#WHERE_CLAUSE#
]]></bm:query-sql>
