<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:parameters>
                <bm:parameter name="return_number" input="false" output="true" outputPath="@return_number"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
            	begin
    				prj_project_workflow_pkg.insert_hl_prj_overrule(
	    					p_project_id => ${@project_id},
	    					p_instance_id => ${@instance_id},
	    					p_from_node_id   => ${@from_node_id}, 
	    					p_node_desc   => ${@node_desc}, 
	                        p_user_id       =>${/session/@user_id},
	                        p_return_reason  =>${@return_reason},
	                        p_return_number  =>${@return_number},
	                        p_node_action_id  =>${@node_action_id} );
    			end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
