<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    Exp_Employee_Assigns_v v
                WHERE
                    v.unit_id          = 5 AND
                    v.unit_enabled     = 'Y' AND
                    v.position_enabled = 'Y' AND
                    v.enabled_flag     = 'Y' AND
                    v.position_name    ='风险评审助理'
                ORDER BY
                    employee_id ASC
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
