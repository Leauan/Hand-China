<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                	pv.*,
                    pv.project_number,
                    pv.project_name,
                    pv.employee_id_of_manager_desc,
                    cv.business_type_n,
                    cv.chance_number,
                    pv.division,
                    pv.division_desc
                    
                FROM
                    prj_project_v pv,
                    prj_chance_v cv
                WHERE
                    pv.chance_id = cv.chance_id
                and pv.project_id= ${@project_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
