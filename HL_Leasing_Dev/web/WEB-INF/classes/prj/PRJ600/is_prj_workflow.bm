<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:fields>
        <bm:field name="prj_workflow"/>
        <bm:field name="workflow_id"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            --modify by wuts  目前只有租赁申请工作流需要
           select 'Y' prj_workflow, workflow_id 
              from zj_wfl_workflow t1  
               where workflow_code in ('WFL_PRJ')
               and workflow_id = ${@workflow_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
