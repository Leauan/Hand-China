<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin
              		cus_app_customer_pkg.app_bp_contact_save(
              				p_prj_contact_info_id =>${@prj_contact_info_id},
                            p_prj_bp_id           =>${@prj_bp_id},
                            p_contact_person       =>${@contact_person},
                            p_cell_phone           =>${@cell_phone},
                            p_comments             =>${@comments},
                            p_contact_person_1     =>${@contact_person_1},
                            p_cell_phone_1         =>${@cell_phone_1},
                            p_comments_1           =>${@comments_1},
                            p_user_id              =>nvl(${/session/@user_id},${@user_id})
                    );
				end;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="prj_contact_info_id" dataType="java.lang.Long" input="true" output="true" outputPath="@prj_contact_info_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
