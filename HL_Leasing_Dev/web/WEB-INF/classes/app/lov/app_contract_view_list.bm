<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    cc.project_number,
                    cc.contract_number,
                    cc.contract_id,
                    cc.bp_id_tenant_n,
                    (SELECT
                        hbm.id_card_no
                    FROM
                        hls_bp_master hbm
                    WHERE
                        hbm.bp_id = cc.bp_id_tenant
                    ) id_card_no
                FROM
                    con_contract_lv cc,
                    con_contract_lease_item ccli
                WHERE
                    cc.contract_id = ccli.contract_id AND
                    cc.contract_id =${/parameter/@contract_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="project_number" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="PROJECT_NUMBER"/>
        <bm:field name="contract_number" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CONTRACT_NUMBER"/>
        <bm:field name="contract_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CONTRACT_ID" required="true"/>
        <bm:field name="bp_id_tenant_n" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_ID_TENANT_N"/>
        <bm:field name="id_card_no" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ID_CARD_NO"/>
    </bm:fields>
</bm:model>
