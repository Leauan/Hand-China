<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t.model_id code,
                    t.description name,
                    t.price guide_price
                FROM
                    hls_car_model_vl t
                WHERE
                    t.enabled_flag = 'Y' AND
                    t.series_id    = ${/parameter/@series_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE"/>
        <bm:field name="name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="NAME"/>
        <bm:field name="guide_price" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="GUIDE_PRICE"/>
    </bm:fields>
</bm:model>
