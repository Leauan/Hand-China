<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    v.code_value AS code ,
                    v.code_value_name AS NAME
                FROM
                    sys_code_values_v v
                WHERE
                    v.code                    = 'HLS211_MARITAL_STATUS' AND
                    v.code_enabled_flag       = 'Y' AND
                    v.code_value_enabled_flag = 'Y'
                ORDER BY
                    v.order_seq
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE"/>
        <bm:field name="name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="NAME"/>
    </bm:fields>
</bm:model>
