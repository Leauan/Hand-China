<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    li.project_id,
                    li.brand_id,
                    li.brand_id_n,--品牌
                    li.series_id,
                    li.series_id_n,
                    li.model_id,
                    li.model_id_n,--车型车系
                    li.guide_price,
                    li.objective_buy_car,
                    li.objective_buy_car_n,
                    li.invoice_price,
                    li.item_frame_number,
                    li.used_evaluation_amout,
                    li.insurance_price,
                    li.travel_tax,
                    li.purchase_tax,
                    li.gps_amount,
                    li.other_amount,
                    li.charge_amount,
                    li.postage,
                    li.lease_times times
                FROM
                    prj_project_lease_item_lv li
                WHERE
                    li.project_id =${@project_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="project_id"/>
        <bm:field name="brand_id"/>
        <bm:field name="brand_id_n"/>
        <bm:field name="series_id"/>
        <bm:field name="series_id_n"/>
        <bm:field name="model_id"/>
        <bm:field name="model_id_n"/>
        <bm:field name="guide_price"/>
        <bm:field name="objective_buy_car"/>
        <bm:field name="objective_buy_car_n"/>
        <bm:field name="invoice_price"/>
        <bm:field name="item_frame_number"/>
        <bm:field name="used_evaluation_amout"/>
        <bm:field name="insurance_price"/>
        <bm:field name="travel_tax"/>
        <bm:field name="purchase_tax"/>
        <bm:field name="gps_amount"/>
        <bm:field name="other_amount"/>
        <bm:field name="charge_amount"/>
        <bm:field name="postage"/>
        <bm:field name="times"/>
    </bm:fields>
</bm:model>
