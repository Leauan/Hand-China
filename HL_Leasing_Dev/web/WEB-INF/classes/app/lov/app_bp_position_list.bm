<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    A.code_value CODE,
                    A.code_value_name NAME
                FROM
                    SYS_CODE_VALUES_V A
                WHERE
                    A.code                   ='PRJ500N_POSITION_TYPE' AND
                    A.code_enabled_flag      ='Y' AND
                    A.code_value_enabled_flag='Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE" required="true"/>
        <bm:field name="name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="NAME"/>
    </bm:fields>
</bm:model>
