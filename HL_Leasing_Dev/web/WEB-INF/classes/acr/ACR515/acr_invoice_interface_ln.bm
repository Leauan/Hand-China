<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    acr_invoice_interface_lv l
                WHERE
                    l.invoice_hd_id IN
                    (SELECT
                        t.invoice_hd_id
                    FROM
                        acr_interface_tmp t
                    WHERE
                        t.session_id = ${/session/@session_id}
                    )
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="invoice_kind_n"/>
        <bm:field name="invoice_date"/>
        <bm:field name="document_number"/>
        <bm:field name="bp_code"/>
        <bm:field name="bp_name"/>
        <bm:field name="bp_tax_registry_num"/>
        <bm:field name="bp_address_phone_num"/>
        <bm:field name="bp_bank_account"/>
        <bm:field name="description"/>
        <bm:field name="vat_red_notice_num"/>
        <bm:field name="ref_vat_invoice_code"/>
        <bm:field name="ref_invoice_number"/>
        <bm:field name="created_by_name"/>
        <bm:field name="confirmed_by_name"/>
        <bm:field name="received_by_name"/>
        <bm:field name="invoice_title"/>
        <bm:field name="invoice_bp_address_phone_num"/>
        <bm:field name="product_code"/>
        <bm:field name="product_name"/>
        <bm:field name="specification"/>
        <bm:field name="uom"/>
        <bm:field name="quantity"/>
        <bm:field name="total_amount" datatype="java.lang.Double"/>
        <bm:field name="tax_type_rate" datatype="java.lang.Double"/>
        <bm:field name="tax_amount" datatype="java.lang.Double"/>
        <bm:field name="discount_amount" datatype="java.lang.Double"/>
        <bm:field name="dicount_tax_amount" datatype="java.lang.Double"/>
        <bm:field name="price" datatype="java.lang.Double"/>
        <bm:field name="discount_rate" datatype="java.lang.Double"/>
        <bm:field name="rec_man"/>
        <bm:field name="rec_man_tax_num"/>
        <bm:field name="del_man"/>
        <bm:field name="del_man_tax_num"/>
        <bm:field name="place_of_sending"/>
        <bm:field name="car_type_num"/>
        <bm:field name="car_ship_tonnage"/>
        <bm:field name="product_info"/>
        <bm:field name="code_version"/>
        <bm:field name="tax_type_code"/>
        <bm:field name="preferential_policy_flag"/>
        <bm:field name="preferential_policy_content"/>
        <bm:field name="zero_tax_rate_flag"/>
        <bm:field name="reduce_amount"/>
    </bm:fields>
</bm:model>
