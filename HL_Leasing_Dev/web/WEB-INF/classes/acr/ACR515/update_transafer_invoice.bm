                <bm:parameter name="user_id" dataType="java.lang.Long" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    ACR515_INVOICE_VAT_PKG.UPDATE_TRANSAFER_INVOICE
