<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
   			BEGIN
                    con_contract_pkg.invoice_account(p_tax_type_rate => ${@tax_type_rate},
                    								 p_product_name => ${@product_name},
                    								 p_invoice_kind => ${@invoice_kind},
                    								 p_vat_interface_status => ${@vat_interface_status},
                    								 p_invoice_date_from => to_date(${@invoice_date_from},'yyyy-mm-dd'),
                    								 p_invoice_date_to => to_date(${@invoice_date_to},'yyyy-mm-dd'));
            END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
