<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select status from fnd_interface_headers #WHERE_CLAUSE#
        	]]></bm:query-sql>
        </bm:operation>
        <!-- -->
        <bm:operation name="insert">
            <bm:parameters>
                <bm:parameter name="session_id"/>
                <bm:parameter name="company_id"/>
                <bm:parameter name="header_id" input="true"/>
                <bm:parameter name="import_type"/>
                <bm:parameter name="user_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                	con_df_car_info_pkg.ins_df_car_interface_temp(p_session_id  =>${/session/@session_id},
                                                                 p_header_id   =>${@header_id},
                                                                 p_company_id  =>${/session/@company_id},
                                                                 p_import_type	=>${@import_type},
                                                                 p_user_id     =>${/session/@user_id});
                end;]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                begin
                    con_df_car_info_pkg.check_df_car_interface_temp(p_session_id=>${/session/@session_id},
										                         p_company_id=>${/session/@company_id},
										                         p_import_type	=>${@import_type},
										                         p_user_id=>${/session/@user_id},
										                         p_return_id => ${@return_id});
                
        	end;]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="return_id" input="false" output="true"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
                begin
                    con_df_car_info_pkg.del_df_car_interface_temp(p_session_id=>${/session/@session_id},
										                         p_company_id=>${/session/@company_id},
										                         p_import_type	=>${@import_type},
										                         p_user_id=>${/session/@user_id});
                
        	end;]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                begin
                    con_df_car_info_pkg.init_df_car_interface_data(p_session_id=>${/session/@session_id},
										                         p_company_id=>${/session/@company_id},
										                         p_import_type	=>${@import_type},
										                         p_user_id=>${/session/@user_id});
                
        	end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="status" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="status"/>
    </bm:fields>
</bm:model>
