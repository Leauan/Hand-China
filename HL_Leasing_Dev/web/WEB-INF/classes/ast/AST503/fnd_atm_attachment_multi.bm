<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:ns2="aurora.database.local.oracle" xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
           select count(1) atm_num from fnd_atm_attachment_multi t 
           where   t.table_name=${@table_name} and  t.table_pk_value=to_char(${@table_pk_value})
        ]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="atm_num" dataType="java.lang.Long" input="true" output="true" outputPath="/parameter/@atm_num"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
