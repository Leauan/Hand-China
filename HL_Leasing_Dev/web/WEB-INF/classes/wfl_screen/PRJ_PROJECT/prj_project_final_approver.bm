<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="PRJ_PROJECT_FINAL_APPROVER" needAccessControl="false">
    <bm:fields>
        <bm:field name="final_approver_record_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="FINAL_APPROVER_RECORD_ID" prompt="PRJ_PROJECT_FINAL_APPROVER.FINAL_APPROVER_RECORD_ID"/>
        <bm:field name="approval_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="APPROVAL_ID" prompt="PRJ_PROJECT_FINAL_APPROVER.APPROVAL_ID"/>
        <bm:field name="approver_user_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="APPROVER_USER_ID" prompt="PRJ_PROJECT_FINAL_APPROVER.APPROVER_USER_ID"/>
        <bm:field name="final_comment" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="FINAL_COMMENT" prompt="PRJ_PROJECT_FINAL_APPROVER.FINAL_COMMENT"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk/>
    </bm:features>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
    			select pfa.final_approver_record_id,
       pfa.approval_id,
       pfa.approver_user_id,
       pfa.final_comment
  from prj_project_final_approver pfa
  where pfa.approval_id= ${@approval_id}
    		]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:primary-key>
        <bm:pk-field name="final_approver_record_id"/>
    </bm:primary-key>
</bm:model>
