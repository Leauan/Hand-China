<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:fields>
        <bm:field name="company_code" databaseType="VARCHAR" datatype="java.lang.String"/>
        <bm:field name="company_short_name" databaseType="VARCHAR" datatype="java.lang.String"/>
        <bm:field name="company_full_name" databaseType="VARCHAR" datatype="java.lang.String"/>
        <bm:field name="start_date_active" databaseType="DATE" datatype="java.sql.Date"/>
        <bm:field name="end_date_active" databaseType="DATE" datatype="java.sql.Date"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    a.company_code,
                    a.company_short_name,
                    a.company_full_name,
                    a.start_date_active,
                    a.end_date_active
                FROM
                    fnd_companies_vl a
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
