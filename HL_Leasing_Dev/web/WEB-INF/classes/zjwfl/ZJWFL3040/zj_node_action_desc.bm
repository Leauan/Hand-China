<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select distinct (node_action_desc),action_type from zj_wfl_workflow_node_action where node_action_desc in('通过','拒绝')]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="node_action_desc" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" physicalName="NODE_ACTION_DESC"/>
        <bm:field name="action_type" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ACTION_TYPE" required="true"/>
    </bm:fields>
</bm:model>
