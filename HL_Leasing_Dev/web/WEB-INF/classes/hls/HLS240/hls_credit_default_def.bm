<bm:model xmlns:e="aurora.service.exception" xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" baseTable="HLS_CREDIT_DEFAULT_DEF" needAccessControl="false">
    <bm:fields>
        <bm:field name="hls_credit_default_def_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="HLS_CREDIT_DEFAULT_DEF_ID" prompt="HLS_CREDIT_DEFAULT_DEF.HLS_CREDIT_DEFAULT_DEF_ID"/>
        <bm:field name="bp_category" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_CATEGORY"/>
        <bm:field name="bp_class" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_CLASS" prompt="HLS_CREDIT_DEFAULT_DEF.BP_CLASS"/>
        <bm:field name="lease_channel" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="LEASE_CHANNEL" prompt="HLS_CREDIT_DEFAULT_DEF.LEASE_CHANNEL"/>
        <bm:field name="valid_from" databaseType="DATE" datatype="java.util.Date" physicalName="VALID_FROM" prompt="HLS_CREDIT_DEFAULT_DEF.VALID_FROM"/>
        <bm:field name="valid_to" databaseType="DATE" datatype="java.util.Date" physicalName="VALID_TO" prompt="HLS_CREDIT_DEFAULT_DEF.VALID_TO"/>
        <bm:field name="credit_limit" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CREDIT_LIMIT" prompt="HLS_CREDIT_DEFAULT_DEF.CREDIT_LIMIT"/>
        <bm:field name="credit_quantity" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CREDIT_QUANTITY" prompt="HLS_CREDIT_DEFAULT_DEF.CREDIT_QUANTITY"/>
        <bm:field name="credit_status" databaseType="VARCHAR2" datatype="java.lang.String" insertExpression="&apos;NORMAL&apos;" physicalName="CREDIT_STATUS" prompt="HLS_CREDIT_DEFAULT_DEF.CREDIT_STATUS"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG" prompt="HLS_CREDIT_DEFAULT_DEF.ENABLED_FLAG"/>
        <bm:field name="external_business_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EXTERNAL_BUSINESS_FLAG" prompt="HLS_CREDIT_DEFAULT_DEF.EXTERNAL_BUSINESS_FLAG"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk/>
    </bm:features>
    <bm:query-fields>
        <bm:query-field field="bp_class" queryOperator="="/>
        <bm:query-field field="lease_channel" queryOperator="="/>
        <bm:query-field name="credit_limit_f" queryExpression="${@credit_limit_f} &gt;= t1.credit_limit"/>
        <bm:query-field name="credit_limit_t" queryExpression="${@credit_limit_t} &lt;= t1.credit_limit"/>
    </bm:query-fields>
    <bm:primary-key>
        <bm:pk-field name="hls_credit_default_def_id"/>
    </bm:primary-key>
    <e:exception-descriptor-config>
        <e:exception-descriptor exception="java.sql.SQLException" handleClass="aurora.database.SQLExceptionDescriptor">
            <e:error-message code="1" message="请勿重复定义"/>
        </e:exception-descriptor>
    </e:exception-descriptor-config>
