<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="YONDA_INTERFACE_LOGS" defaultOrderBy="t1.interface_date desc">
    <bm:fields>
        <bm:field name="interface_log_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="INTERFACE_LOG_ID" prompt="YONDA_INTERFACE_LOGS.INTERFACE_LOG_ID"/>
        <bm:field name="interface_type" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="INTERFACE_TYPE" prompt="YONDA_INTERFACE_LOGS.INTERFACE_TYPE"/>
        <bm:field name="source_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SOURCE_ID" prompt="YONDA_INTERFACE_LOGS.SOURCE_ID"/>
        <bm:field name="source_table" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SOURCE_TABLE" prompt="YONDA_INTERFACE_LOGS.SOURCE_TABLE"/>
        <bm:field name="interface_deal_message" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="INTERFACE_DEAL_MESSAGE" prompt="YONDA_INTERFACE_LOGS.INTERFACE_DEAL_MESSAGE"/>
        <bm:field name="itfc_exception" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ITFC_EXCEPTION" prompt="YONDA_INTERFACE_LOGS.ITFC_EXCEPTION"/>
        <bm:field name="external_itfc_num" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EXTERNAL_ITFC_NUM" prompt="YONDA_INTERFACE_LOGS.EXTERNAL_ITFC_NUM"/>
        <bm:field name="interface_date" databaseType="DATE" datatype="java.util.Date" physicalName="INTERFACE_DATE" prompt="YONDA_INTERFACE_LOGS.INTERFACE_DATE"/>
        <bm:field name="journal_num" expression="(select a.journal_num from hls_journal_header a where a.journal_header_id=t1.source_id)"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="interface_log_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field name="journal_num" queryExpression="exists (select 1 from hls_journal_header a where a.journal_header_id=t1.source_id and a.journal_num=${@journal_num} )"/>
        <bm:query-field field="external_itfc_num" queryOperator="like"/>
        <bm:query-field name="start_date" queryExpression="trunc(t1.interface_date)&gt;=to_date(${@start_date},&apos;yyyy-mm-dd&apos;)"/>
        <bm:query-field name="end_date" queryExpression="trunc(t1.interface_date)&lt;=to_date(${@end_date},&apos;yyyy-mm-dd&apos;)"/>
    </bm:query-fields>
</bm:model>
