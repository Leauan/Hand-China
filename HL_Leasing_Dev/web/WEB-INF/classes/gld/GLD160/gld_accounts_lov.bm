<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                select t1.ACCOUNT_ID,t1.ACCOUNT_CODE,t1.DESCRIPTION from gld_accounts_vl t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="account_id" forDisplay="false" forQuery="false" prompt="科目ID"/>
        <bm:field name="account_code" forDisplay="true" forQuery="true" prompt="科目代码"/>
        <bm:field name="description" forDisplay="true" forQuery="true" prompt="科目名称"/>
    </bm:fields>
    <bm:query-fields>
        <bm:query-field name="account_code" queryExpression="t1.account_code like &apos;%&apos;||${@account_code}||&apos;%&apos;"/>
        <bm:query-field name="description" queryExpression="t1.description like &apos;%&apos;||${@description}||&apos;%&apos;"/>
    </bm:query-fields>
    <!-- <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="(t1.account_set_id=${@set_of_books_id})"/>
    </bm:data-filters> -->
</bm:model>
