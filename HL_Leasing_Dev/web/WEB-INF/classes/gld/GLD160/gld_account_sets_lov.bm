<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT t1.account_set_id,t1.account_set_code,t1.account_set_name
                FROM gld_account_sets t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="account_set_id" forDisplay="false" forQuery="false" prompt="科目表ID"/>
        <bm:field name="account_set_code" forDisplay="true" forQuery="true" prompt="科目表代码"/>
        <bm:field name="account_set_name" forDisplay="true" forQuery="true" prompt="科目表名称"/>
    </bm:fields>
    <bm:query-fields>
        <bm:query-field name="account_set_code" queryExpression="t1.account_set_code like &apos;%&apos;||${@account_set_code}||&apos;%&apos;"/>
        <bm:query-field name="account_set_name" queryExpression="t1.account_set_name like &apos;%&apos;||${@account_set_name}||&apos;%&apos;"/>
    </bm:query-fields>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="1 =1"/>
    </bm:data-filters>
</bm:model>
