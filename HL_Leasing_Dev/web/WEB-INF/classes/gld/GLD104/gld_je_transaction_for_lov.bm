<ns1:model xmlns:ns1="http://www.aurora-framework.org/schema/bm" alias="t1" extend="gld.GLD104.gld_je_transaction" extendMode="reference">
    <ns1:fields>
        <ns1:field name="je_transaction_code" displayWidth="200" forDisplay="true" forQuery="true"/>
        <ns1:field name="je_transaction_desc" displayWidth="250" forDisplay="true" forQuery="true"/>
        <ns1:field name="enabled_flag" displayWidth="250" forDisplay="false" forQuery="false"/>
    </ns1:fields>
    <ns1:query-fields>
        <ns1:query-field field="je_transaction_code" queryOperator="like"/>
        <ns1:query-field field="je_transaction_desc" queryOperator="like"/>
    </ns1:query-fields>
    <ns1:data-filters>
        <ns1:data-filter name="query" expression="t1.enabled_flag=&apos;Y&apos;"/>
    </ns1:data-filters>
</ns1:model>
