<ns1:model xmlns:ns1="http://www.aurora-framework.org/schema/bm" baseTable="GLD_PERIOD_STATUS">
    <ns1:fields>
        <ns1:field databaseType="VARCHAR2" datatype="java.lang.String" name="period_set_code"/>
        <ns1:field databaseType="NUMBER" datatype="java.lang.Long" name="company_id"/>
        <ns1:field databaseType="VARCHAR2" datatype="java.lang.String" name="period_name"/>
        <ns1:field databaseType="NUMBER" datatype="java.lang.Long" name="internal_period_num"/>
        <ns1:field databaseType="VARCHAR2" datatype="java.lang.String" name="period_status_code"/>
    </ns1:fields>
    <ns1:primary-key>
        <ns1:pk-field name="company_id"/>
        <ns1:pk-field name="internal_period_num"/>
        <ns1:pk-field name="period_set_code"/>
    </ns1:primary-key>
    <ns1:ref-fields/>
    <ns1:operations>
    <ns1:operation name="query">
    <ns1:query-sql>
    <![CDATA[ 
    Select g.company_id,
       g.period_set_code,
       g.period_name,
       g.internal_period_num,
       g.period_status_code
  From GLD_PERIOD_STATUS g
 Order By g.internal_period_num
    ]]>
    </ns1:query-sql>
    </ns1:operation>
    </ns1:operations>
</ns1:model>
