<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="success_flag" dataType="java.lang.String" input="false" output="true"/>
                <bm:parameter name="messages" dataType="java.lang.String" input="false" output="true"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    tre_loan_con_repayment_pkg.reverse_loan_con_repayment
                    (
                        p_repayment_id =>${@repayment_id},
                        p_reversed_date=>to_date(${@reversed_date},'yyyy-mm-dd'),
                        p_user_id      =>${/session/@user_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
