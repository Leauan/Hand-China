<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    field,
                    no
                FROM
                    (SELECT
                        field,
                        rownum no
                    FROM
                        (SELECT
                            ${@year}
                            || lpad(LEVEL, 2, '0') AS field
                        FROM
                            dual CONNECT BY LEVEL <= 12
                        ORDER BY
                            field
                        ) t
                    )
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
