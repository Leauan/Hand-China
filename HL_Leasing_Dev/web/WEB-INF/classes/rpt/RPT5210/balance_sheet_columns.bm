<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                 SELECT
                    TO_CHAR(fdate,'yyyy-mm-dd') field,
                    no 
                FROM
                    (SELECT
                        fdate,
                        rownum no
                    FROM
                        (SELECT
                            TRUNC(to_date(${@start_date},'YYYY-MM-DD')) + LEVEL - 1 AS fdate
                        FROM
                            dual CONNECT BY LEVEL <= ${@days}
                        ) t
                    )
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
