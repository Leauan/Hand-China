<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                   to_char(sysdate,'yyyy') as year,
                   to_char(sysdate,'mm') as current_month,
                   sysdate as current_period
                FROM
                    dual
           ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
