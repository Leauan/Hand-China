<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[Select *
  From con_contract t
 Where t.contract_id In (Select cc.contract_id
                           From con_contract_cashflow cc
                          Where TRUNC(Sysdate) - TRUNC(cc.due_date) = 1
                            And cc.cf_item = 1
                            And cc.overdue_status = 'Y'
						)
   And t.overdue_status = 'Y'
   And t.contract_status = 'INCEPT'
   And t.data_class = 'NORMAL'
   
   ]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            begin
                HL_SMS_SEND_PKG.sys_collection_t2_sms_list(p_contract_id =>${@contract_id}, p_user_id =>${/session/@user_id}, p_sms_id =>${@sms_id});
           end;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="sms_id" dataType="java.lang.Long" input="true" output="true" outputPath="/parameter/@sms_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
