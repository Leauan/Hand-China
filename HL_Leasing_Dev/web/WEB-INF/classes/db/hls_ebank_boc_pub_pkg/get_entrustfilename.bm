<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    hls_ebank_boc_pub_pkg.get_entrustfilename( p_batch_id => ${/session/@batch_id}, p_company_id => ${/session/@company_id}, p_user_id => ${/session/@user_id}, p_filename => ${@filename} );
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="filename" input="false" output="true" outputPath="/parameter/@filename"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
