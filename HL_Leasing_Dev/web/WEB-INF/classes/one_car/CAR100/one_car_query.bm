<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" dataSourceName="one" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT DISTINCT
                    t1.fld_make,
                    t2.fld_model,
                    t3.fld_trim,
                    t3.fld_ccsj,
                    t4.fld_price,
                    t3.is_delete_column
                FROM
                    tab_sCarMake T1,
                    tab_sCarModel T2,
                    tab_sCarTrim t3,
                    tab_sCarGuidedPrice t4
                WHERE
                    T1.fld_makeid                                  =t2.fld_makeid AND
                    T2.fld_modelid                                 =t3.fld_modelid AND
                    T3.fld_makeid                                  =t1.fld_makeid AND
                    t4.fld_trimid                                  =t3.fld_trimid --AND
                    --CONVERT(VARCHAR(10),t3.update_time_column,120) = CONVERT(VARCHAR(10),GETDATE(),120)
                ORDER BY
                    t1.fld_make
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
