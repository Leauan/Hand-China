<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" dataSourceName="one">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t.fld_trimid,
                    t.fld_price,
                    t.is_delete_column
                FROM
                    tab_scarguidedprice t
                WHERE
                    CONVERT(VARCHAR(10),t.update_time_column,120) = CONVERT(VARCHAR(10),GETDATE(),120)
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
