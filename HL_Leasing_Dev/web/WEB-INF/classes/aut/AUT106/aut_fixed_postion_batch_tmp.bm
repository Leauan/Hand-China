<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:o="aurora.database.local.oracle" xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="AUT_FIXED_POSTION_BATCH_TMP">
    <bm:fields>
        <bm:field name="record_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="RECORD_ID" prompt="AUT_FIXED_POSTION_BATCH_TMP.RECORD_ID"/>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" insertExpression="${/session/@session_id}" physicalName="SESSION_ID" prompt="AUT_FIXED_POSTION_BATCH_TMP.SESSION_ID"/>
        <bm:field name="position_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="POSITION_ID" prompt="AUT_FIXED_POSTION_BATCH_TMP.POSITION_ID"/>
        <bm:field name="position_id_c" expression="(select t.position_code from exp_org_position_vl t where t.position_id = t1.position_id)" forInsert="false" forUpdate="false"/>
        <bm:field name="position_id_n" expression="(select t.description from exp_org_position_vl t where t.position_id = t1.position_id)" forInsert="false" forUpdate="false"/>
        <bm:field name="company_id_n" expression="(select fc.company_full_name from exp_org_position_vl t,fnd_companies fc where t.COMPANY_ID = fc.company_id and t.position_id = t1.position_id)" forInsert="false" forUpdate="false"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="record_id"/>
    </bm:primary-key>
    <bm:data-filters>
        <bm:data-filter name="session_id" enforceOperations="query" expression="t1.session_id=${/session/@session_id}"/>
    </bm:data-filters>
</bm:model>
