<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select cd.*,rownum rn,contract_number||' '||bp_name contract_num
  					from csh_payment_excel_lv cd
 					where cd.payment_req_id in
       				(select t.payment_req_id
          			from csh_payment_export_tmp t
         			where t.session_id = ${/session/@session_id})
         			order by cd.payment_req_id desc]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
