<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                begin
                	update csh_payment_req_hd set
                		print_flag 		 = 'Y',
                		last_update_date = sysdate,
                		last_updated_by  = ${/session/@user_id}
                	where payment_req_id in (${:@payment_req_id});			              
                end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
