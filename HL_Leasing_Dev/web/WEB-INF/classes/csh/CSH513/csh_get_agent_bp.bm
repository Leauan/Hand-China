<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select bm.bp_name, bm.bp_code, bm.bp_id
  from hls_bp_master bm
  #WHERE_CLAUSE#]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="bp_name" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="BP_NAME" prompt="经销商名称"/>
        <bm:field name="bp_code" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="BP_CODE" prompt="经销商编码"/>
        <bm:field name="bp_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="BP_ID"/>
    </bm:fields>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="bm.bp_category = &apos;AGENT&apos; and bm.enabled_flag = &apos;Y&apos; and bm.agent_type = &apos;AGENT_GRT_DEPOSIT&apos;"/>
    </bm:data-filters>
    <bm:query-fields>
        <bm:query-field field="bp_name" queryOperator="like"/>
        <bm:query-field field="bp_code" queryOperator="like"/>
    </bm:query-fields>
</bm:model>
