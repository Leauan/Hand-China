<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="HL_TRANSFER_FEE_EXPORT">
    <bm:fields>
        <bm:field name="export_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="EXPORT_ID" prompt="HL_TRANSFER_FEE_EXPORT.EXPORT_ID"/>
        <bm:field name="fee_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="FEE_ID" prompt="HL_TRANSFER_FEE_EXPORT.FEE_ID"/>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID" prompt="HL_TRANSFER_FEE_EXPORT.SESSION_ID"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="export_id"/>
    </bm:primary-key>
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                INSERT
                INTO
                    HL_TRANSFER_FEE_EXPORT
                    (
                        session_id,
                        export_id,
                        fee_id,
                        creation_date,
                        created_by,
                        last_update_date,
                        last_updated_by
                    )
                    VALUES
                    (
                        ${/session/@session_id},
                        HL_TRANSFER_FEE_EXPORT_S.NEXTVAL,
                        ${@fee_id},
                        sysdate,
                        1,
                        sysdate,
                        1
                    )
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
     		DELETE
                FROM
                    HL_TRANSFER_FEE_EXPORT t
                WHERE
                    t.session_id = ${/session/@session_id}    
     	]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
