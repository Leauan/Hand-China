<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="HL_TRANSFER_FEE_HD">
    <bm:fields>
        <bm:field name="hd_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="HD_ID" prompt="HL_TRANSFER_FEE_HD.HD_ID"/>
        <bm:field name="fee_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="FEE_ID" prompt="HL_TRANSFER_FEE_HD.FEE_ID"/>
        <bm:field name="last_updated_date" databaseType="DATE" datatype="java.util.Date" physicalName="LAST_UPDATED_DATE" prompt="HL_TRANSFER_FEE_HD.LAST_UPDATED_DATE"/>
        <bm:field name="amount" databaseType="NUMBER" datatype="java.lang.Long" physicalName="AMOUNT" prompt="HL_TRANSFER_FEE_HD.AMOUNT"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="hd_id"/>
    </bm:primary-key>
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                BEGIN
                HL_TRANSFER_FEE_PKG.update_transfer_fee_info(p_hd_id=> ${/parameter/@v_id},
                                            p_fee_id =>${@fee_id},
                                            p_user_id =>${/session/@user_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                     select hl_transfer_fee_hd_s.nextval into  ${@v_id} from dual;
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="v_id" output="true" outputPath="/parameter/@v_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                HL_TRANSFER_FEE_PKG.save_transfer_fee_info(p_hd_id=> ${/parameter/@v_id},
                                            p_user_id =>${/session/@user_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
