<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[SELECT
                    hl.fee_id,
                    hl.bank_code,
                    hl.transfer_fee_amount,
                  hl.transfer_fee_date,
                   journal_num,
                  NVL(hl.post_gl_status_desc,'未生成')post_gl_status_desc,
                  t.export_id,
                  t.session_id
                FROM
                    hl_transfer_fee_lv hl,
                    HL_TRANSFER_FEE_EXPORT t
                WHERE
                    hl.fee_id =t.fee_id AND
                    t.session_id=${/session/@session_id}]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter inputPath="/session/@session_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="fee_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="FEE_ID" required="true"/>
        <bm:field name="bank_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BANK_CODE"/>
        <bm:field name="transfer_fee_amount" databaseType="NUMBER" datatype="java.lang.Long" physicalName="TRANSFER_FEE_AMOUNT"/>
        <bm:field name="transfer_fee_date" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TRANSFER_FEE_DATE"/>
        <bm:field name="journal_num" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="JOURNAL_NUM"/>
        <bm:field name="post_gl_status_desc" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="POST_GL_STATUS_DESC"/>
        <bm:field name="export_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="EXPORT_ID" required="true"/>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID"/>
    </bm:fields>
</bm:model>
