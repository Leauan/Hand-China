<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                begin
                	update csh_transaction_return cr set
                		cr.print_flag 	 = 'Y',
                		cr.last_update_date = sysdate,
                		cr.last_updated_by  = ${/session/@user_id}
                	where cr.return_id in (${:@return_id});			              
                end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
