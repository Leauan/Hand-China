<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    bp_bank_account_num,
                    ct.csh_bp_name
                FROM
                    CSH_TRANSACTION ct #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="bp_bank_account_num" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" prompt="客户账号"/>
        <bm:field name="csh_bp_name" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" prompt="客户名称"/>
    </bm:fields>
    <bm:query-fields>
        <bm:query-field field="bp_bank_account_num" queryOperator="="/>
        <bm:query-field field="csh_bp_name" queryOperator="like"/>
    </bm:query-fields>
</bm:model>
