<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="YONDA_HLS_TL_ITFC_HEADER">
    <bm:fields>
        <bm:field name="tl_itfc_header_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="TL_ITFC_HEADER_ID" prompt="YONDA_HLS_TL_ITFC_HEADER.TL_ITFC_HEADER_ID"/>
        <bm:field name="trx_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TRX_CODE" prompt="YONDA_HLS_TL_ITFC_HEADER.TRX_CODE"/>
        <bm:field name="trx_status" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TRX_STATUS" prompt="YONDA_HLS_TL_ITFC_HEADER.TRX_STATUS"/>
        <bm:field name="version" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="VERSION" prompt="YONDA_HLS_TL_ITFC_HEADER.VERSION"/>
