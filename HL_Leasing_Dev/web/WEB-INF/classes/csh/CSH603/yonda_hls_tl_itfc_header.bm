        <bm:field name="tl_level" databaseType="NUMBER" datatype="java.lang.Long" physicalName="TL_LEVEL" prompt="YONDA_HLS_TL_ITFC_HEADER.TL_LEVEL"/>
        <bm:field name="req_sn" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="REQ_SN" prompt="YONDA_HLS_TL_ITFC_HEADER.REQ_SN"/>
        <bm:field name="signed_msg" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SIGNED_MSG" prompt="YONDA_HLS_TL_ITFC_HEADER.SIGNED_MSG"/>
        <bm:field name="business_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BUSINESS_CODE" prompt="YONDA_HLS_TL_ITFC_HEADER.BUSINESS_CODE"/>
        <bm:field name="merchant_id" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="MERCHANT_ID" prompt="YONDA_HLS_TL_ITFC_HEADER.MERCHANT_ID"/>
        <bm:field name="settday" databaseType="DATE" datatype="java.util.Date" physicalName="SETTDAY" prompt="YONDA_HLS_TL_ITFC_HEADER.SETTDAY"/>
        <bm:field name="submit_time" databaseType="DATE" datatype="java.util.Date" physicalName="SUBMIT_TIME" prompt="YONDA_HLS_TL_ITFC_HEADER.SUBMIT_TIME"/>
        <bm:field name="total_item" databaseType="NUMBER" datatype="java.lang.Long" physicalName="TOTAL_ITEM" prompt="YONDA_HLS_TL_ITFC_HEADER.TOTAL_ITEM"/>
        <bm:field name="total_sum" databaseType="NUMBER" datatype="java.lang.Double" physicalName="TOTAL_SUM" prompt="YONDA_HLS_TL_ITFC_HEADER.TOTAL_SUM"/>
