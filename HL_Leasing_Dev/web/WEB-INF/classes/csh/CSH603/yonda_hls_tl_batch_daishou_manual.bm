<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin
            		yonda_tl_itfc_header_pkg.batch_daishou_manual(
            			p_merchantid =>${@merchantid},
            			p_user_id =>nvl(${/session/@user_id},-999),
            			p_session_id =>nvl(${/session/@session_id},-999),
            			p_tl_itfc_header_id =>${@tl_itfc_header_id}
        			); 
            	end;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="tl_itfc_header_id" dataType="java.lang.Long" input="false" output="true" outputPath="@tl_itfc_header_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
