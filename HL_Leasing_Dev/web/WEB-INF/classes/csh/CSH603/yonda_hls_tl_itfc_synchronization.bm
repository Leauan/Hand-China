<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    h.tl_itfc_header_id
                FROM
                    yonda_hls_tl_itfc_header h
                WHERE
                    h.trx_status IN ('TRANSFOR', 'DEALING')
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="tl_itfc_header_id" databaseType="NUMBER" datatype="java.lang.Long"/>
    </bm:fields>
</bm:model>
