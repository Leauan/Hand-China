<bm:model xmlns:e="aurora.service.exception" xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features">
    <bm:operations>
        <bm:operation name="insert">
            <bm:parameters>
                <bm:parameter name="debit_number" datatype="java.lang.String" input="true"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
        		begin
        		
				  hls_bill99_request_pkg.update_debit_number(p_interface_id =>${@interface_id},
                              								  p_debit_number =>${@debit_number});

				end;
        	]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:parameters>
                <bm:parameter name="flag" output="true" outputPath="/parameter/@flag"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
        		begin
        		
				  hls_bill99_request_pkg.nc_protocol_parse_xml(p_response_xml  =>${@response_xml},
		                              p_function_name =>${@function_name},
		                              p_contract_id   =>${@contract_id},
                                      p_protocol_number =>${@protocol_number},
		                              p_request_id   =>${@request_id},
		                              p_user_id      =>-1);

				end;
        	]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="flag" output="true" outputPath="/parameter/@flag"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
        		begin
        		
				  hls_bill99_request_pkg.check_embk_parse_xml(p_response_xml  =>${@response_xml},
		                              p_function_name =>${@function_name},
		                              p_request_id   =>${@request_id},
		                              p_interface_id  =>${@interface_id},
		                              p_user_id      =>-1,
		                              p_flag     =>${@flag});

				end;
        	]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select hls_bill99_request_pkg.check_embk_parse_xml(p_response_xml  =>${@response_xml},
						                              p_function_name =>${@function_name},
						                              p_request_id   =>${@request_id},
						                              p_interface_id  =>${@interface_id},
						                              p_user_id      =>-1) as flag from dual]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="flag" required="true"/>
    </bm:fields>
</bm:model>
