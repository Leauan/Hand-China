<doc watermark='永达融资' >
	<header>
		<p>
		    <img src='YONDA_LOGO.jpg' />
			<pbdr>
				<bottom />
			</pbdr>
			 <t bold="true" font="宋体" fontSize="18" fontColor="#000"></t>
			 
		</p>
	</header>
	<footer >
		<p align="left">
        <pbdr>
            <top space="1" sz="6" color="auto" value="single"/>
        </pbdr>
        </p>
	</footer>
	<body>
		<p align="center" line='900' >
		    <t bold="true" font="宋体" fontSize="50"  fontColor="#000">合同信息要素表</t>
		</p>
		<p indLeft="-2">
			<t bold="true" font="宋体" fontSize="30"   fontColor="#000">一、合同信息</t>
		</p>
		<#if model.getObject('/model/contract_info_for_print').getChilds()??>
		     <#list  model.getObject("/model/contract_info_for_print").getChilds() as contract_info_r>
		       <tbl indLeft="-2" border="true" align="center">
		       <tr height="1">
				  <tc width="2.5" >
					  <p><t fontSize="16" font="宋体">项目总价款</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('lease_item_amount')!''}</t></p>
				  </tc> 
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">融资额</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('finance_amount')!''}</t></p>
				  </tc>
				    <tc width="2.5">
					  <p><t fontSize="16" font="宋体">首付比例</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('down_payment_ratio')!''}</t></p>
				  </tc> 
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">首付金额</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('down_payment')!''}</t></p>
				  </tc> 
			   </tr>
			   <tr height="1">
			      <tc width="2.5">
					  <p><t fontSize="16" font="宋体">还款期数</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('lease_times')!''}</t></p>
				  </tc> 
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">支付频率</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('annual_pay_times_n')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">租赁期限(年)</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('lease_term')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">租赁年利率</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('int_rate')!''}</t></p>
				  </tc>
			   </tr>
			    <tr height="1">
			      <tc width="2.5">
					  <p><t fontSize="16" font="宋体">保证金比例</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('deposit_ratio')!''}</t></p>
				  </tc> 
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">保证金</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('deposit')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">手续费比例</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('lease_charge_ratio')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">手续费</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('lease_charge')!''}</t></p>
				  </tc>
			   </tr>
			<tr height="1">
			      <tc width="2.5">
					  <p><t fontSize="16" font="宋体">期末残值比例</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('residual_ratio')!''}</t></p>
				  </tc> 
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">期末残值</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('residual_value')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">厂商返利比例</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('hd_user_col_n01')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">厂商返利</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('net_other_fee3')!''}</t></p>
				  </tc>
			   </tr>
				<tr height="1">
			      <tc width="2.5">
					  <p><t fontSize="16" font="宋体">租前息</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('interim_rental')!''}</t></p>
				  </tc> 
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">租赁合同总金额</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('contract_amount')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">我司实际动用资金</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('hd_user_col_n03')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">项目粗利</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('hd_user_col_n04')!''}</t></p>
				  </tc>
			   </tr>
			 	<tr height="1">
			      <tc width="2.5">
					  <p><t fontSize="16" font="宋体">IRR</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('irr')!''}</t></p>
				  </tc> 
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">每期租金</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('contract_amount')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体">支付类型</t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体">${contract_info_r.getString('pay_type')!''}</t></p>
				  </tc>
				   <tc width="2.5">
					  <p><t fontSize="16" font="宋体"></t></p>
				  </tc>
				  <tc width="2.5">
					  <p><t fontSize="16" font="宋体"></t></p>
				  </tc>
			   </tr>
		       </tbl> 
		</#list>
		</#if>
		<p indLeft="-2">
			<t bold="true" font="宋体"  fontSize="30"  fontColor="#000">二、签署方信息</t>
		</p>
	 ${model.getObject('/model/bp_info_path/record/@bp_info')!''}
	</body>
</doc>
