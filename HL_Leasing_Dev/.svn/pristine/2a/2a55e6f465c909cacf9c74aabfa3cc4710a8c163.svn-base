<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="hls_bp_master">
    <bm:fields>
        <bm:field name="bp_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="BP_ID" prompt="承租人id"/>
        <bm:field name="bp_code" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="BP_CODE" prompt="承租人代码"/>
        <bm:field name="bp_name" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="BP_NAME" prompt="承租人名称"/>
    </bm:fields>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="bp_category=&apos;AGENT&apos;"/>
    </bm:data-filters>
    <bm:query-fields>
        <bm:query-field name="bp_code" queryExpression="bp_code like &apos;%&apos;|| ${@bp_code} ||&apos;%&apos;"/>
        <bm:query-field name="bp_name" queryExpression="bp_name like &apos;%&apos;|| ${@bp_name} ||&apos;%&apos;"/>
    </bm:query-fields>
</bm:model>
