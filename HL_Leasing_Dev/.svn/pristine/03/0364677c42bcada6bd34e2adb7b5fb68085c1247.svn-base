<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="ZX_QUERY_TMP">
    <bm:fields>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID" prompt="ACR_INTERFACE_TMP.SESSION_ID"/>
        <bm:field name="payment_seq_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PAYMENT_SEQ_ID" prompt="付款ID"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:operations>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
    			
    				delete from csh_payment_export_tmp  where session_id = ${/session/@session_id}
    			
    		]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
    			
    			  insert into csh_payment_export_tmp(session_id,payment_req_id) values(${/session/@session_id},${@payment_req_id})
    			
    		]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
