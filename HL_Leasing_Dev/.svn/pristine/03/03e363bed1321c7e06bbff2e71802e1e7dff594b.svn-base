<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure/>
    <a:view>
        <script><![CDATA[
		]]></script>
        <a:grid id="ast_car_trailer_grid_id" bindTarget="ast_car_trailer_result_ds" height="200" marginWidth="42" navBar="true">
            <a:columns>
                <a:column name="trailer_date" prompt="拖车日期" renderer="Aurora.formatDate" width="100"/>
                <a:column name="trailer_place" prompt="拖车地点" width="200"/>
                <a:column name="trailer_partner" prompt="合作伙伴" width="100"/>
                <a:column name="trailer_fee" align="right" prompt="拖车费用" renderer="Aurora.formatMoney" width="100"/>
                <a:column name="trailer_result" prompt="拖车结果" width="284"/>
                <a:column name="disposal_scheme" prompt="处置方案" width="284"/>
            </a:columns>
            <a:editors>
                <a:numberField id="ast_car_trailer_nf"/>
                <a:textField id="ast_car_trailer_tf"/>
                <a:datePicker id="ast_car_trailer_dp"/>
            </a:editors>
        </a:grid>
        <a:hBox/>
    </a:view>
</a:screen>
