<a:screen xmlns:a="http://www.aurora-framework.org/application" customizationEnabled="true" dynamiccreateenabled="true" trace="true">
    <a:view>
        <a:link id="con600_overdue_analyse_date_link" model="cont.CON600.contract_overdue_analyse" modelaction="execute"/>
        <script><![CDATA[
			window['${/parameter/@layout_code}_user_button1_layout_dynamic_click'] = function() {
                var analyse_date = $('CONTRACT_OVERDUE_QUERY_F_QUERY_HD__ds').getCurrentRecord().get('analyse_date');
                if(!analyse_date){
                    Aurora.showMessage('提示','分析日期不能为空！');
                    return;
                }
                window['${/parameter/@layout_code}_lock_layout_dynamic_window']();
                    Aurora.request({
                        url: $('con600_overdue_analyse_date_link').getUrl(),
                        para: {
                            analyse_date: analyse_date
                        },
                        success: function() {
							window['${/parameter/@layout_code}_QUERY_LAYOUT_DYNAMIC_CLICK']();
							window['${/parameter/@layout_code}_unlock_layout_dynamic_window']();
                        },
                        scope: this
                    });
            };
            Aurora.onReady(function(){
                //Aurora.center('loginForm');
				window['${/parameter/@layout_code}_user_button1_layout_dynamic_click']();
            });
        ]]></script>
        <a:screen-include screen="modules/cont/CON500/con_contract_get_layout_code.screen"/>
    </a:view>
</a:screen>
