<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                  SELECT
                    to_char(nvl(rate,0)*100,'FM90.09') ||'%' field,
                    no 
                FROM
                    (SELECT
                        rate,
                        order_no no
                    FROM hls_linkage_bp_rate t
                    where t.hd_id =${@hd_id}
                    )
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
