<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT * FROM hls_bp_master_org_v t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="organization_code" queryExpression="t1.organization_code=${@organization_code} and  rownum&lt;2"/>
    </bm:query-fields>
</bm:model>
