<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select b.record_id,
				       b.contract_id,
				       b.bp_category,
				       (select c.description
			          from hls_bp_category c
			         where c.bp_category = b.bp_category) bp_category_desc,
				       m.bp_id,
				       m.bp_code,
				       m.bp_name,
				       m.extra_nam,
				       m.bp_category,
				       m.bp_type,
				       m.bp_title
				  from con_contract_bp b, hls_bp_master m
				 where b.bp_id = m.bp_id
				 and b.contract_id=${@contract_id}
             ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
