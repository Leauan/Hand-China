<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT * FROM con_workday_query tt #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="bp_id" queryExpression="tt.bp_id = ${@bp_id}"/>
    </bm:query-fields>
    <bm:data-filters>
        <bm:query-field name="query" expression="(tt.session_user_id = ${/session/@user_id})"/>
    </bm:data-filters>
</bm:model>
