<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="xml_temp" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@xml_temp"/>
                <bm:parameter name="transaction_id" dataType="java.lang.Long" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_EBANK_CUP_PUB_PKG.QUERY_MANUAL_PUB_TRANSACTION
                    (
                        p_xml_temp=>${@xml_temp},
                        p_user_id=>${/session/@user_id},
                        p_session_id=>${/session/@session_id},
                        p_transaction_id=>${@transaction_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
