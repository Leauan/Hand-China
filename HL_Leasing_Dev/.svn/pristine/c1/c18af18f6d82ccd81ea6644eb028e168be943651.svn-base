        <a:grid id="ast_car_condition_grid_id" bindTarget="ast_car_condition_result_ds" height="200" marginWidth="30" navBar="true">
            <a:toolBar>
                <a:button type="add"/>
                <a:button type="delete"/>
            </a:toolBar>
            <a:columns>
                <a:column name="maintenance_date" editor="ast_car_condition_dp" prompt="保养日期" renderer="Aurora.formatDate" width="100"/>
                <a:column name="maintenance_place" editor="ast_car_condition_tf" prompt="保养地点" width="250"/>
                <a:column name="maintenance_content" editor="ast_car_condition_tf" prompt="保养内容" width="418"/>
                <a:column name="mileage" align="right" editor="ast_car_condition_nf" prompt="行驶里程(KM)"/>
                <a:column name="salvage_value" align="right" editor="ast_car_condition_nf" prompt="残值" renderer="Aurora.formatMoney" width="100"/>
                <a:column name="vendibility" align="right" editor="ast_car_condition_nf" prompt="市场价值" renderer="Aurora.formatMoney" width="100"/>
            </a:columns>
            <a:editors>
                <a:numberField id="ast_car_condition_nf"/>
                <a:textField id="ast_car_condition_tf"/>
                <a:datePicker id="ast_car_condition_dp"/>
            </a:editors>
        </a:grid>
        <a:hBox/>
    </a:view>
</a:screen>
