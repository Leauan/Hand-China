<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    hls_acr_ebanking_pkg.save_import_data(p_header_id => ${@header_id},
                                                      p_user_id => ${/session/@user_id},
                                                      p_output_id	=>${@output_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    hls_acr_ebanking_pkg.csh_write_off_ebank_input(p_output_id=>${@output_id},
                                      p_user_id  => ${/session/@user_id},
                                      p_company_id  => ${/session/@company_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
