<?xml version="1.0" encoding="UTF-8"?>
<a:screen xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure><![CDATA[
    ]]></a:init-procedure>
    <a:view template="default">
        <script language="javascript"><![CDATA[
            function goBack() {
                if ('${/parameter/@winId}') {
                    $('${/parameter/@winId}').close();
                } else {
                    history.back();
                }
            }
        ]]></script>
        <a:vBox style="margin:30px;">
            <div style="padding:10px;background-color:#ffc;border:2px solid #d14836;"><![CDATA[
                ${/parameter/@msg}
            ]]></div>
            <a:button click="goBack" text="HLS.RETURN"/>
        </a:vBox>
    </a:view>
</a:screen>
