<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT t.cf_item AS cf_item, t.description AS cf_item_desc
                  FROM HLS_CASHFLOW_ITEM t
                WHERE t.cf_item IN (301, 302, 303, 304)
                and t.enabled_flag = 'Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
