<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="HLS_CCB_MANUAL_TMP">
    <bm:fields>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID" prompt="HLS_CCB_MANUAL_TMP.SESSION_ID"/>
        <bm:field name="contract_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CONTRACT_ID" prompt="HLS_CCB_MANUAL_TMP.CONTRACT_ID"/>
        <bm:field name="cashflow_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CASHFLOW_ID" prompt="HLS_CCB_MANUAL_TMP.CASHFLOW_ID"/>
        <bm:field name="rec_amount" databaseType="NUMBER" datatype="java.lang.Double" physicalName="REC_AMOUNT" prompt="HLS_CCB_MANUAL_TMP.REC_AMOUNT"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
    			INSERT INTO HLS_CCB_MANUAL_TMP
					  (session_id,
					   contract_id,
					   cashflow_id,
					   rec_amount)
					VALUES
					  (${/session/@session_id},
					   ${@contract_id},
					   ${@cashflow_id},
					   ${@manual_rec_amount})
    			
    		]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="delete"><![CDATA[
        	delete HLS_CCB_MANUAL_TMP where session_id = ${/session/@session_id}
        ]]></bm:operation>
    </bm:operations>
</bm:model>
