<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="project_id" dataType="java.lang.String" input="true" output="false" required="true"/>
                <bm:parameter name="query_category" dataType="java.lang.Long" input="true" output="false" required="true"/>
                <bm:parameter name="bp_id" dataType="java.lang.Long" input="false" output="true" outputPath="/parameter/@bp_id"/>
                <bm:parameter name="bp_name" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@bp_name"/>
                <bm:parameter name="id_number" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@id_number"/>
                <bm:parameter name="cell_phone" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@cell_phone"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    CREDIT_QUERY_PKG.GET_BP_NAME_ID_NUMBER_PHONE
                    (
                        p_project_id=>${@project_id},
                        p_query_category=>${@query_category},
                        p_user_id=>${/session/@user_id},
                        p_bp_id=>${@bp_id},
                        p_bp_name=>${@bp_name},
                        p_id_number=>${@id_number},
                        p_cell_phone=>${@cell_phone}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
