<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select t1.contact_person as person, t1.email as mail
			  from hls_bp_master_contact_info t1
			 where t1.ref_v01 = 'Y'
			 order by person
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
