<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="xml_temp" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@xml_temp"/>
                <bm:parameter name="dd_bank_account_num" dataType="java.lang.String" input="true" output="false"/>
                <bm:parameter name="dd_bank_account_name" dataType="java.lang.String" input="true" output="false"/>
                <bm:parameter name="id_type" dataType="java.lang.String" input="true" output="false"/>
                <bm:parameter name="id_number" dataType="java.lang.String" input="true" output="false"/>
                <bm:parameter name="bank_reserved_phone" dataType="java.lang.String" input="true" output="false"/>
				<bm:parameter name="direct_debit_bank_id" dataType="java.lang.Long" input="true" output="false"/>
				<bm:parameter name="contract_number" dataType="java.lang.String" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_EBANK_CUP_PUB_PKG.HL_CUP_SIGN_REQUEST
                    (
                        p_xml_temp=>${@xml_temp},
                        p_dd_bank_account_num=>${@dd_bank_account_num},
                        p_dd_bank_account_name=>${@dd_bank_account_name},
                        p_id_type=>${@id_type},
                        p_id_number=>${@id_number},
                        p_bank_reserved_phone=>${@bank_reserved_phone},
						p_direct_debit_bank_id=>${@direct_debit_bank_id},
						p_contract_number=>${@contract_number},
                        p_user_id=>${/session/@user_id},
                        p_session_id=>${/session/@session_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
