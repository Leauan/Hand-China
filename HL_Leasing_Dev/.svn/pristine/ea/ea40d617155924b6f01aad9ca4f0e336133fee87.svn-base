<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:parameters>
                <bm:parameter name="trs_status"/>
            </bm:parameters>
            <bm:query-sql><![CDATA[
        
             SELECT
                 t1.INTERFACE_ID,
                 t1.COMPANY_ID,
                 t1.CASHFLOW_ID,
                 t1.PRIORITY,
                 t1.FUNNAM,
                 t1.DATTYP,
                 t1.LGNNAM,
                 t1.BUSCOD,
                 t1.BUSMOD,
                 t1.MODALS,
                 t1.TRSTYP,
                 t1.C_TRSTYP,
                 t1.EPTDAT,
                 t1.DBTACC,
                 t1.BBKNBR,
                 t1.BANKAREA,
                 t1.SUM,
                 t1.TOTAL,
                 t1.CCYNBR,
                 t1.CURRENCY,
                 t1.YURREF,
                 t1.MEMO,
                 t1.ACCNBR,
                 t1.CLTNAM,
                 t1.TRSAMT,
                 t1.BNKFLG,
                 t1.EACBNK,
                 t1.EACCTY,
                 t1.TRSDSP,
                 t1.RETCOD,
                 t1.ERRMSG,
                 t1.REQNBR,
                 t1.OPRDAT,
                 t1.TRS_STATUS,
                 t1.CONFIRM_DATE,
                 t1.SUBMIT_DATE,
                 t1.CSH_TRANSACTION_ID
             FROM
                 HLS_EBANK_INTERFACE t1
             WHERE
                 t1.company_id =${/session/@company_id} AND
                 t1.trs_status = ${@trs_status}
             ORDER BY
                 interface_id
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
