<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                UPDATE
                    con_contract t
                SET
                    t.rebate_month            = ${@rebate_month},
                    t.commission_confirm_flag = ${@commission_confirm_flag},
                    t.last_updated_by         = ${/session/@user_id},
                    t.last_update_date        = sysdate
                WHERE
                    t.contract_id = ${@contract_id}
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="rebate_month"/>
                <bm:parameter name="commission_confirm_flag"/>
                <bm:parameter name="contract_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
