<?xml version="1.0" encoding="UTF-8"?>
<a:screen xmlns:a="http://www.aurora-framework.org/application" cache="false" default-dispatch="${/model/url/@URL}" dispatch-style="redirect" trace="true">
    <a:init-procedure>
        <a:model-query model="csh.CSH542.deposit_return_print" rootPath="deposit_return_path"/>
        <!-- <a:model-query model="csh.CSH502.csh_payment_req_ln_upd_query" rootPath="payment_req_ln_path"/>
        <a:model-query model="csh.CSH502.csh_payment_req_approved" rootPath="approved_info_path"/> -->
        <a:model-execute model="csh.CSH542.update_deposit_return_print"/>
    </a:init-procedure>
    <a:view>
        <style media="print"><![CDATA[
	        .PageNext
	        {
	            page-break-after: always;
	        }
		]]></style>
        <style type="text/css"><![CDATA[
			table
			{
			border-collapse:collapse;
			border:none;
			}
			table td{ height:32px; border:#CCCCCC 1px solid;}
    	]]></style>
        <ajax-init/>
        <a:freeMarker><![CDATA[
        <#if  model.getObject('/model/deposit_return_path').getChilds()??>
		<#list  model.getObject("/model/deposit_return_path").getChilds() as deposit_return_rec>
		 <#if (deposit_return_rec.getString('rk')!'')!='1'> 
		    <div class="PageNext">
            </div>
         </#if>
		 <P align="left" style="margin-left:60px;" >
            <B>
                <FONT face="楷体">浙江智慧普华融资租赁有限公司</FONT>
            </B>
        </P>
        <P align="center" >
            <B>
                <FONT face="楷体" size="7">经销商保证金退还打印单</FONT>
            </B>
        </P>
       <table border="1" width="1000" style="margin-left:60px"  valign="middle">
            <tr align="center">
             <td width="15%">申请人：</td>
             <td colspan="2">${deposit_return_rec.getString('submitted_by_n')!''}</td> 
             <td width="15%">业务部门</td>
             <td colspan="2">${deposit_return_rec.getString('unit_name')!''}</td> 
           </tr>
		    <tr align="center">
             <td>收款单位名称</td>
             <td colspan="2">${deposit_return_rec.getString('bank_account_name')!''}</td> 
             <td rowspan="3">付款内容</td>
             <td rowspan="3" colspan="2">${deposit_return_rec.getString('bp_name')!''}</td> 
           </tr>
           <tr align="center">
             <td>账号</td>
             <td colspan="2">${deposit_return_rec.getString('bank_account_num')!''}</td> 
             
           </tr>
           <tr align="center">
             <td>开户银行</td>
             <td colspan="2">${deposit_return_rec.getString('bank_branch_name')!''}</td> 
           </tr>
           <tr align="center">
             <td>人民币(大写)</td>
             <td colspan="3">${deposit_return_rec.getString('return_amount_big')!''}</td> 
             <td width="15%">人民币</td>
             <td width="15%">${deposit_return_rec.getString('this_return_amount')!''}</td> 
           </tr>
           <tr align="center">
             <td>结算方式</td>
             <td colspan="5">电汇</td>
           </tr>
           <tr align="center">
             <td>计划付款日</td>
             <td colspan="3">${deposit_return_rec.getString('creation_date')!''}</td>
             <td>应付款总额</td>
             <td>${deposit_return_rec.getString('this_return_amount')!''}</td>
           </tr>
           <tr align="center">
             <td>备注</td>
             <td colspan="5">${deposit_return_rec.getString('notes')!''}</td>
           </tr>
           <tr align="center">
             <td>风险部长审批</td>
             <td>审批日期</td>
             <td>${deposit_return_rec.getString('create_date_fmt10')!''}</td>
             <td>审批意见</td>
             <td colspan="2">${deposit_return_rec.getString('comment_text_out10')!''}</td>
           </tr>
           <tr align="center">
             <td>总经理审批</td>
             <td>审批日期</td>
             <td>${deposit_return_rec.getString('create_date_fmt20')!''}</td>
             <td>审批意见</td>
             <td colspan="2">${deposit_return_rec.getString('comment_text_out20')!''}</td>
           </tr>
           <tr align="center">
             <td>财务审批</td>
             <td>审批日期</td>
             <td>${deposit_return_rec.getString('create_date_fmt30')!''}</td>
             <td>审批意见</td>
             <td colspan="2">${deposit_return_rec.getString('comment_text_out30')!''}</td>
           </tr>
           <tr align="center">
             <td>财务部长确认</td>
             <td>审批日期</td>
             <td>${deposit_return_rec.getString('create_date_fmt40')!''}</td>
             <td>审批意见</td>
             <td colspan="2">${deposit_return_rec.getString('comment_text_out40')!''}</td>
           </tr>
      </table>
		</#list>
		</#if>
		 ]]></a:freeMarker>
        <script><![CDATA[
            window.onload = window.print();
        ]]></script>
    </a:view>
</a:screen>
