<a:screen xmlns:c="aurora.application.action" xmlns:s="aurora.plugin.script" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure>
        <a:model-query fetchAll="true" model="sys.SYS890.hls_sys_role_home_page_function" rootPath="role_home_page_function_path"/>
        <s:server-script import="home_page.js"/>
    </a:init-procedure>
    <a:view>
        <link href="${/request/@context_path}/images/newmain/css_o.css" rel="stylesheet" type="text/css"/>
        <style><![CDATA[
        	.list_box_n {
				height:130px;
				cursor:pointer;
			}
			
			.list_box_n_d{
			    width:180px;
			    height:65px;
			}
        ]]></style>
        <script><![CDATA[
            function hitPage(function_code) {
                top.mainScreen_hitPage(function_code);
            }
            
            function old_mian_page() {
                window.location.href = 'welcome.screen';
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="home_page_temp_ds"/>
        </a:dataSets>
        <a:screenBody>
            <table id="home_page_table_id" border="0" cellpadding="0" cellspacing="0"><![CDATA[
            ]]></table>
        </a:screenBody>
        <script><![CDATA[
            function init() {
                var e = document.getElementById('home_page_table_id');
                var screenWidth = document.body.offsetWidth - 15;
                e.style.width = screenWidth + "px";
            }
            window.onresize = init;
            init();
        ]]></script>
        <a:dataSets/>
    </a:view>
</a:screen>
