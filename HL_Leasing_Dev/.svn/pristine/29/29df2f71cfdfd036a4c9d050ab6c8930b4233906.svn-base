<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select 
            		a.transaction_id,
					a.transaction_type,	
            		a.status 
				from CSH_TRANSACTION a 
				where a.transaction_type='RISK'
			]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
