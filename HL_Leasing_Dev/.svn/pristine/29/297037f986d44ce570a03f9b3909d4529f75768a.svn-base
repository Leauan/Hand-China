<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="et_agreement_id" dataType="java.lang.Long" input="true" output="true" outputPath="/parameter/@et_agreement_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                BEGIN
                    con_contract_et_pkg.save_contract_et_hd(
                    	p_et_agreement_id  => ${@et_agreement_id},
                        p_contract_id   => ${@contract_id},
                        p_termination_date => to_date(${@termination_date},'yyyy-mm-dd'),
                        p_et_type          => ${@et_type},
                        p_user_id  => ${/session/@user_id},
                        p_et_profile  => ${@et_profile},
                        p_et_ratio    => ${@et_ratio},
                        p_et_fee      => ${@et_fee});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
