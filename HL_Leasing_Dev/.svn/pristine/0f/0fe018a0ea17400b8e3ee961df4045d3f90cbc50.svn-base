<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="HLS_CAR_ORGANIZATION">
    <bm:fields>
        <bm:field name="organization_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="ORGANIZATION_ID" prompt="HLS_CAR_ORGANIZATION.ORGANIZATION_ID"/>
        <bm:field name="organization_code" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="ORGANIZATION_CODE" prompt="事业部代码"/>
        <bm:field name="organization_name" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" physicalName="ORGANIZATION_NAME" prompt="事业部名称"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG" prompt="HLS_CAR_ORGANIZATION.ENABLED_FLAG"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DESCRIPTION" prompt="HLS_CAR_ORGANIZATION.DESCRIPTION"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="organization_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field field="organization_code" queryOperator="like"/>
        <bm:query-field field="organization_name" queryOperator="like"/>
    </bm:query-fields>
    <bm:data-filters>
        <bm:data-filter name="query" expression="t1.enabled_flag=&apos;Y&apos;"/>
    </bm:data-filters>
</bm:model>
