<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:parameters>
                <bm:parameter name="interim_rental" dataType="java.lang.Double" input="false" output="true" outputPath="@interim_rental"/>
                <bm:parameter name="discount" dataType="java.lang.Double" input="false" output="true" outputPath="@discount"/>
                <bm:parameter name="discount_limit_total" dataType="java.lang.Double" input="false" output="true" outputPath="@discount_limit_total"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_CALC_ESTIMATE_PKG.CALC_QUOTATION
                    (
                        p_calc_session_id=>${@calc_session_id},
                        p_price_list=>${@price_list},
                        p_product_plan_id=>${@product_plan_id},
                        p_int_rate_display=>${@int_rate_display},
                        p_down_payment_ratio=>${@down_payment_ratio},
                        p_balloon_ratio=>${@balloon_ratio},
                        p_deposit_ratio=>${@deposit_ratio},
                        p_lease_times=>${@lease_times},
                        p_lease_charge_ratio=>${@lease_charge_ratio},
                        p_user_id=>${/session/@user_id},
                        p_interim_rental=>${@interim_rental},
                        p_discount=>${@discount},
                        p_discount_limit_total=>${@discount_limit_total},
                        p_business_fee =>${@business_fee}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
