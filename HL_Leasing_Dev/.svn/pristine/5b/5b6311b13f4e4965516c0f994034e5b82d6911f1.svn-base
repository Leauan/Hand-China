<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" dataSourceName="one">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    tt.fld_modelid,
                    tt.fld_model,
                    tt.fld_serial,
                    tt.fld_type,
                    tt.fld_makeid,
                    tt.is_delete_column
                FROM
                    tab_sCarModel tt
                WHERE
                    CONVERT(VARCHAR(10),tt.update_time_column,120) = CONVERT(VARCHAR(10),GETDATE(),120)
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
