<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="error_message" dataType="java.lang.String" input="true" output="false" required="true"/>
                <bm:parameter name="batch_id" dataType="java.lang.Long" input="true" output="false" required="true"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    CREDIT_QUERY_PKG.CREDIT_UPDATE_ERROR_MESSAGE
                    (
                        p_error_message=>${@error_message},
                        p_user_id=>${/session/@user_id},
                        p_batch_id=>${@batch_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
