<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin 
				   cfl_fnd_sc_score_pkg.calculate_prj_score_init(p_project_id  =>${@project_id},
				                                                 p_user_id     =>${/session/@user_id},
				                                                 p_score_num   =>${@score_num},
				                                                 p_result_id   =>${@result_id});
				end;                                                        	
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="result_id" output="true" outputPath="/parameter/@result_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
