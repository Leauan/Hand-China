<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features">
    <bm:fields>
        <bm:field name="result_id"/>
        <bm:field name="result_errormasg"/>
        <bm:field name="result_code"/>
        <bm:field name="result_state"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="result_id"/>
    </bm:primary-key>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t1.result_id,
                    t1.result_errormasg,
                    t1.result_code,
                    t1.result_state
                FROM
                    gh_nc_inter_return t1
                WHERE
                    t1.result_id = ${@result_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
