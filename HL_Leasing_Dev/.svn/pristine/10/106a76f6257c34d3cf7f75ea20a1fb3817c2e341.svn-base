<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
               
                    SELECT
                  hbm.work_city registered_place,
                   hbm.work_city_n registered_place_n
                FROM
                    sys_user su,
                    hls_bp_master_lv hbm
                WHERE
                    su.bp_category = 'AGENT' AND
                    su.user_id     =${/session/@user_id} AND
                    su.bp_id       = hbm.bp_id
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
