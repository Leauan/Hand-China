<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
          begin
           hls_product_grant_area_pkg.to_area(p_area_id          => ${@area_id},
								              p_parent_area_id   => ${@parent_area_id},
								              p_area_type        => ${@area_type},
								              p_ebank_id         => ${@ebank_id},
								              p_ebank_num        => ${@ebank_num},
								              p_notes            => ${@notes},
								              p_ebank_spd_flag	 => ${@ebank_spd_flag},
								              p_user_id          => ${/session/@user_id});
          end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
