<a:service xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure>
        <s:server-script><![CDATA[
            importPackage(java.io);
            importPackage(com.test);
            try {
						    var logger = $logger("server-script");
                var xml_temp;
                var transaction_id;
                var seqno;
                var deal_result;
                var user_id;
                var parameter = $ctx.parameter;
                logger.severe(111);
                var manual_pub = $bm('db.hls_ebank_cup_pub_pkg.get_manual_pub_transaction');
				logger.severe(222);
                var manual_pub_result = manual_pub.execute({
                    "xml_temp": xml_temp,
                    "transaction_id": transaction_id,
                    "seqno": seqno,
                    "user_id": parameter.user_id,
                    "session_id": parameter.session_id
            
                });
				logger.severe(333);
                var hlCupManual = new HlCupManual();
                var result = hlCupManual.TestPayReq(parameter.xml_temp);
                var deal_transaction = $bm('db.hls_ebank_cup_pub_pkg.deal_create_transaction_result');
                deal_transaction.execute({
                    "xml_clob": result,
                    "transaction_id": parameter.transaction_id,
                    "seqno": parameter.seqno,
                    "deal_result": deal_result,
                    "user_id": parameter.user_id,
                    "session_id": parameter.session_id
                });
                //println("deal_result" + parameter.deal_result);
            } catch (e) {

                logger.severe(e.message);
                println("错误信息" + e);
            }
        ]]></s:server-script>
    </a:init-procedure>
</a:service>
