<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                select a.bp_category,a.description from hls_bp_category a where a.enabled_flag='Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="bp_category" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" physicalName="bp_category"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" forDisplay="true" forQuery="true" prompt="承租人类别"/>
    </bm:fields>
    <bm:query-fields>
        <bm:query-field name="bp_category" dataType="java.lang.String" queryExpression="h.bp_category = ${@bp_category}"/>
    </bm:query-fields>
</bm:model>
