<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t.class_id value_code,t.description value_name
                FROM
                    hls_stat_class t
                WHERE
                    t.parent_class_id = 156 AND
                    t.class_code      = ${@class_code}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="value_code" databaseType="NUMBER" datatype="java.lang.Long"/>
		<bm:field name="value_name" databaseType="VARCHAR2"/>
    </bm:fields>
</bm:model>
