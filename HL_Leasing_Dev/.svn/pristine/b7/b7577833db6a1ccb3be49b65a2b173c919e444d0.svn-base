            function item_no_validator(record, name, value) {
                var count = 0;
                if (value) { //进行校验
                    Aurora.request({
                        url: $('lease_item_exists_check_link').getUrl(),
                        para: {
                            item_frame_number: value
                        },
                        success: function(res) {
                            count = res.result.record.v_count;
            
                        },
                        failure: function() {
            
                           },
                        error: function() {
            
                           },
                        scope: this,
                        sync: true
                    });
                } else {
                    return true;
                }
                if (count == 0) {
                    return '此车架号不在库存中，请检查库存';
                } else {
                    return true;
                }
            }
            
