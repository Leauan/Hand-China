<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure/>
    <a:view>
        <script><![CDATA[
		]]></script>
        <a:dataSets>
            <a:dataSet id="ast_car_trailer_result_ds" model="ast.AST501.ast_car_trailer" selectable="true">
                <a:fields>
                    <a:field name="ast_car_trailer_id"/>
                    <a:field name="item_detail_id"/>
                    <a:field name="trailer_date"/>
                    <a:field name="trailer_place"/>
                    <a:field name="trailer_partner"/>
                    <a:field name="trailer_fee"/>
                    <a:field name="trailer_result"/>
                    <a:field name="disposal_scheme"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:grid id="ast_car_trailer_grid_id" bindTarget="ast_car_trailer_result_ds" height="200" navBar="true" width="1095">
            <a:toolBar>
                <a:button type="add"/>
                <a:button type="delete"/>
            </a:toolBar>
            <a:columns>
                <a:column name="trailer_date" editor="ast_car_trailer_dp" prompt="拖车日期" renderer="Aurora.formatDate" width="100"/>
                <a:column name="trailer_place" editor="ast_car_trailer_tf" prompt="拖车地点" width="200"/>
                <a:column name="trailer_partner" editor="ast_car_trailer_tf" prompt="合作伙伴" width="100"/>
                <a:column name="trailer_fee" align="right" editor="ast_car_trailer_nf" prompt="拖车费用" renderer="Aurora.formatMoney" width="100"/>
                <a:column name="trailer_result" editor="ast_car_trailer_tf" prompt="拖车结果" width="284"/>
                <a:column name="disposal_scheme" editor="ast_car_trailer_tf" prompt="处置方案" width="284"/>
            </a:columns>
            <a:editors>
                <a:numberField id="ast_car_trailer_nf"/>
                <a:textField id="ast_car_trailer_tf"/>
                <a:datePicker id="ast_car_trailer_dp"/>
            </a:editors>
        </a:grid>
        <a:hBox/>
    </a:view>
</a:screen>
