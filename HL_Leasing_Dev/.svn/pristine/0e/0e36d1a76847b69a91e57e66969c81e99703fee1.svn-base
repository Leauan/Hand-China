<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select h.node_id,h.node_seq,h.node_desc,h.node_type from hls_document_node h where h.enabled_flag = 'Y' order by h.node_seq asc
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
