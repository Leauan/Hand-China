    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                DECLARE
                BEGIN
                    INSERT
                    INTO
                        con_contract_mortgage
                        (
                            contract_id,
                            con_mortgage_id,
                            item_detail_id,
                            mortgage_register_no,
                            mortgage_register_date,
                            mortgage_register_undo_date,
                            mortgage_name,
                            value,
                            note,
                            mortgage_register_fee,
                            location,
                            mortgage_power_name,
                            mortgagor_name,
                            created_by,
                            creation_date,
                            last_updated_by,
                            last_update_date
                        )
                        VALUES
                        (
                            ${/parameter/@contract_id},
                            con_contract_mortgage_s.nextval,
                            ${/parameter/@item_detail_id},
                            ${@mortgage_register_no},
                            to_date(${@mortgage_register_date}, 'yyyy-mm-dd'),
                            to_date(${@mortgage_register_undo_date}, 'yyyy-mm-dd'),
                            ${@mortgage_name},
                            ${@value},
                            ${@note},
                            ${@mortgage_register_fee},
                            ${@location},
                            ${@mortgage_power_name},
                            ${@mortgagor_name},
                            ${/session/@user_id},
                            sysdate,
                            ${/session/@user_id},
                            sysdate
                        )
                        RETURNING con_mortgage_id
                    INTO
                        ${@con_mortgage_id};
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="con_mortgage_id" dataType="java.lang.Long" input="false" output="true" outputPath="@con_mortgage_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    UPDATE
                        con_contract_mortgage t1
                    SET
                        t1.item_detail_id              =${/parameter/@item_detail_id},
                        t1.mortgage_register_no        =${@mortgage_register_no},
                        t1.mortgage_register_date      =to_date(${@mortgage_register_date},'yyyy-mm-dd'),
                        t1.mortgage_register_undo_date =to_date(${@mortgage_register_undo_date},'yyyy-mm-dd'),
                        t1.mortgage_name               =${@mortgage_name},
                        t1.value                       =${@value},
                        t1.note                        =${@note},
                        t1.mortgage_register_fee       =${@mortgage_register_fee},
                        t1.location                    =${@location},
                        t1.mortgage_power_name         =${@mortgage_power_name},
                        t1.mortgagor_name              =${@mortgagor_name},
                        t1.last_updated_by             =${/session/@user_id},
                        t1.last_update_date            =sysdate
                    WHERE
                        t1.con_mortgage_id=${@con_mortgage_id} ;
                END;
            ]]></bm:update-sql>
        </bm:operation>
