<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT TO_CHAR(SYSDATE, 'YYYY-MM-DD hh24:mi:ss') sys_date FROM dual
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="sys_date"/>
    </bm:fields>
</bm:model>
