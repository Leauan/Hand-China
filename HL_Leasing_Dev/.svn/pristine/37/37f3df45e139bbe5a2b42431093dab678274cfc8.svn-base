<a:screen xmlns:a="http://www.aurora-framework.org/application" customizationEnabled="true" trace="true">
    <a:init-procedure>
        <a:model-query fetchAll="true" model="gld.gld_currency_vl" rootPath="currency"/>
    </a:init-procedure>
    <a:view>
        <a:link id="sys_service_welcome_link" url="${/request/@context_path}/welcome.screen"/>
        <a:link id="csh_transaction_receipt_write_off_detail_rd_link" url="${/request/@context_path}/modules/csh/CSH513/csh_transaction_receipt_write_off_detail_rd.screen"/>
        <a:link id="csh_transaction_receipt_write_off_detail_link" url="${/request/@context_path}/modules/csh/CSH513/csh_transaction_receipt_write_off_detail.screen"/>
        <script><![CDATA[
            function dateValidator(record, name, value) { //日期校验方法
                if (name == 'transaction_date_from' || name == 'transaction_date_to') {
                    var start_date = Aurora.formatDate(record.get('transaction_date_from'));
                    var end_date = Aurora.formatDate(record.get('transaction_date_to'));
                    if ( !! end_date) { //由于结束日期非必填，只有在结束日期填了才进行比较
                        if (!compareDate(start_date, end_date)) {
                            return '${l:CSH510.CSH_RECEIPT_DATE_CHECK}'; //校验不通过返回字符串
                        }
                    }
                    return true; //校验通过返回true
                }
            }
            
            function compareDate(start, end) {
                if (start > end) {
                    return false;
                }
                return true;
            }
            
            function csh513_receipt_query() {
                var record = $('csh_transaction_receipt_write_off_query_ds').getAt(0);
                var dirty_flag = 'N';
                for (var name in record.data) {
                    var value = record.get(name);
                    if (value != 'undefined' && value != null && value != '') {
                        dirty_flag = 'Y';
                    }
                }
                if (dirty_flag == 'N') {
                    Aurora.showConfirm('${l:PROMPT}', '${l:CSH510.CSH_RECEIPT_QUERY_INFORMATION}', function() {
                        return;
                    }, function() {
                        $('csh_transaction_receipt_write_off_result_ds').query();
                    }, null, null);
                } else {
                    $('csh_transaction_receipt_write_off_result_ds').query();
                }
            }
            
            function csh513_receipt_reset() {
                $('csh_transaction_receipt_write_off_query_ds').reset();
            }
            
            function csh513_receipt_back() {
                history.go(-1);
            }
            
            function csh513_receipt_forward() {
                history.go(1);
            }
            
            function csh513_receipt_quit() {
                location.href = $('sys_service_welcome_link').getUrl();
                parent.shMenu('show');
            }
            
            function seedetail_transaction_num(value, record, name) {
                var status = record.get('status');
                var cf_df_flag = record.get('cf_df_flag'); //df 标志
                if (record.get('transaction_id')) {
                    if (status == 'APPROVING') { //审批中的跳入只读页面
                        url = $('csh_transaction_receipt_write_off_detail_rd_link').getUrl() + '?transaction_id=' + record.get('transaction_id') + '&transaction_type=' + record.get('transaction_type') + '&auto_filt_penalty=' + record.get('auto_filt_penalty') + '&auto_filt_prinicipal_interest=' + record.get('auto_filt_prinicipal_interest');
                    } else {
                        url = $('csh_transaction_receipt_write_off_detail_link').getUrl() + '?transaction_id=' + record.get('transaction_id') + '&bp_id1=${/parameter/@bp_id}' + '&transaction_type=' + record.get('transaction_type') + '&auto_filt_penalty=' + record.get('auto_filt_penalty') + '&auto_filt_prinicipal_interest=' + record.get('auto_filt_prinicipal_interest') + '&bp_limit_flag=${/parameter/@bp_limit_flag}&cf_df_flag=' + cf_df_flag;
                    }
                    return '<a href="javascript:openWindow(\'' + url + '\');">' + value + '</a>';
                }
            }
            
            function openWindow(url) {
                var limit_flag = '${/parameter/@limit_flag}' || 'N';
                var limit2_flag = '${/parameter/@limit2_flag}' || 'N';
                new Aurora.Window({
                    id: 'receipt_write_off_detail_link_winid',
                    params: {
                        winId: 'receipt_write_off_detail_link_winid',
                        limit_flag: limit_flag,
                        limit2_flag: limit2_flag
                    },
                    url: url,
                    title: '${l:CSH513.CSH_RECEIPT_WRITE_OFF}',
                    fullScreen: true
                }).on('close', function() {
                    csh513_receipt_query();
                });
            }
            
            
            function csh513_write_off() {
                var records = $('csh_transaction_receipt_write_off_result_ds').getAll();
                for (var i = 0;i < records.length;i++) {
                    var record_d = records[i];
                    if (record_d.dirty) {
                        Aurora.showInfoMessage('提示', '请先保存！');
                        return false;
                    }
                }
            
                var record = $('csh_transaction_receipt_write_off_result_ds').getCurrentRecord();
                var status = record.get('status');
                if (record.get('transaction_id')) {
                    if (status == 'APPROVING'){ //审批中的跳入只读页面
                        url = $('csh_transaction_receipt_write_off_detail_rd_link').getUrl() + '?transaction_id=' + record.get('transaction_id')+'&transaction_type='+record.get('transaction_type')+'&auto_filt_penalty='+record.get('auto_filt_penalty')+'&auto_filt_prinicipal_interest='+record.get('auto_filt_prinicipal_interest');    
                    }else{
                    	url = $('csh_transaction_receipt_write_off_detail_link').getUrl() + '?transaction_id=' + record.get('transaction_id')+ '&bp_id1=${/parameter/@bp_id}' +'&transaction_type='+record.get('transaction_type')+'&auto_filt_penalty='+record.get('auto_filt_penalty')+'&auto_filt_prinicipal_interest='+record.get('auto_filt_prinicipal_interest')+'&bp_limit_flag=${/parameter/@bp_limit_flag}';    
                    }
                }
                var limit_flag='${/parameter/@limit_flag}'||'N';
                var limit2_flag='${/parameter/@limit2_flag}'||'N';
                new Aurora.Window({
                    id: 'receipt_write_off_detail_link_winid',
                    params: {
                        winId: 'receipt_write_off_detail_link_winid',
                        limit_flag: limit_flag,
                        limit2_flag:limit2_flag
                    },
                    url: url,
                    title: '${l:CSH513.CSH_RECEIPT_WRITE_OFF}',
                    fullScreen: true
                }).on('close',function(){
					    csh513_receipt_query();
				});
            }
            function construct_from_to() {
                var config = ['transaction_num', 'bank_slip_num', 'transaction_amount', 'currency_code', 'bank_account_code', 'bp_code', 'bp_name', 'bp_id', 'transaction_date'];
                var pattern = ['_from', '_to'];
                var obj = {};
                for (var i = 0,
                    j = config.length;i < j;i++) {
                    for (var m = 0,
                        n = pattern.length;m < n;m++) {
                        obj[config[i] + pattern[m]] = config[i] + pattern[n - 1 - m];
                    }
                }
                return obj;
            }
            
            function csh513_receipt_write_off_update(ds, record, name, value, oldvalue) {
                var from_to = construct_from_to();
                var opposite_name = from_to[name];
                if (!Ext.isEmpty(opposite_name)) {
                    var opposite_value = record.get(opposite_name);
                    if (!Ext.isEmpty(value) && Ext.isEmpty(opposite_value)) {
                        record.set(opposite_name, value);
                    }
                }
            }
            
            function load_result_ds(){
                var rec = $('csh_transaction_receipt_write_off_result_ds').getCurrentRecord();
                if(rec && '${/parameter/@limit3_flag}'){
                    rec.set('auto_filt_penalty','${/parameter/@limit3_flag}');
                }
            }
            
            Aurora.onReady(
            
            function() {
                if ( !! '${/parameter/@deposit_flag}') {
                    if ('${/parameter/@deposit_flag}' == 'Y') {
                        var rec = $('csh_transaction_receipt_write_off_query_ds').getCurrentRecord();
                        rec.set('bp_id_from', '${/parameter/@bp_id}');
                        rec.set('bp_code_from', '${/parameter/@bp_code}');
                        rec.set('bp_name_from', '${/parameter/@bp_name}');
                        rec.set('bp_id_to', '${/parameter/@bp_id}');
                        rec.set('bp_code_to', '${/parameter/@bp_code}');
                        rec.set('bp_name_to', '${/parameter/@bp_name}');
                        rec.set('receipt', 'N');
                        rec.set('advance_receipt', 'N');
                        rec.set('deposit', 'DEPOSIT');
                    }
                }
            });
            
            function summary_Renderer(datas, name) {
                var sum = 0;
                var sum1 = 0;
                if (name == 'unwrite_off_amount') {
                    for (var i = 0;i < datas.length;i++) {
                        var record = datas[i];
                        var amount = record.get("unwrite_off_amount");
                        amount = parseFloat(amount);
                        if (!isNaN(amount)) {
                            sum += amount ;
                        }
                    }
                return '<font color="RED">' + Aurora.formatNumber(sum, 2) + '</font>';
                } 
                if (name == 'transaction_amount') {
                    for (var i = 0;i < datas.length;i++) {
                        var record1 = datas[i];
                        var amount = record1.get("transaction_amount");
                        amount = parseFloat(amount);
                        if (!isNaN(amount)) {
                            sum1 += amount ;
                        }
                    }
                return '<font color="RED">' + Aurora.formatNumber(sum1, 2) + '</font>';
                } 
                if (name == 'bp_name') {
                    for (var i = 0;i < datas.length;i++) {
                        var record1 = datas[i];
                        var amount = record1.get("transaction_amount");
                        amount = parseFloat(amount);
                        if (!isNaN(amount)) {
                            sum1 += amount ;
                        }
                    }
                    return '合计:';
                }
            }
            
            //add by 8754 批量核销
            
            function csh513_receipt_batch_write_off() {
                
                var records = $('csh_transaction_receipt_write_off_result_ds').getAll();
                for (var i = 0;i < records.length;i++) {
                    var record_d = records[i];
                    if (record_d.dirty) {
                        Aurora.showInfoMessage('提示', '请先保存！');
                        return false;
                    }
                }
                var record = $('csh_transaction_receipt_write_off_result_ds').getSelected()[0];
                var description = record.get('description');
                if (record.get('lease_channel')!='01') {
                    Aurora.showInfoMessage('提示', 'DF以外的业务无法进行批量核销!');
                    return;
                }
                if (Ext.isEmpty(description)) {
                    Aurora.showInfoMessage('提示', '未匹配核销现金流，无法自动核销！');
                    return;
                }
                function batch_write_off() {
                    Aurora.request({
                        url: '${/request/@context_path}/autocrud/csh.CSH513.csh_write_off_tmp_execute/update',
                        para: {
                            transaction_id: record.get('transaction_id')
                        },
                        success: function() {
                            //跳转页面
                            var url = $('csh_transaction_receipt_write_off_detail_link').getUrl() + '?transaction_id=' + record.get('transaction_id') + '&bp_id1=${/parameter/@bp_id}' + '&transaction_type=' + record.get('transaction_type') + '&auto_filt_penalty=' + record.get('auto_filt_penalty') + '&auto_filt_prinicipal_interest=' + record.get('auto_filt_prinicipal_interest') + '&bp_limit_flag=${/parameter/@bp_limit_flag}';
                            openWindow(url);
                        }
                    });
                }
            
                Aurora.showConfirm('提示', '是否确认批量核销!', function ok() {
                    batch_write_off();
                }, function cancel() {
            
                   });
            }
            
            //摘要修改 addby wuts
            
            function description_editor(record, name) {
                if (name == 'description' &&record.get('transaction_type')=='RECEIPT') {
                    if (record.get('transaction_amount') == record.get('unwrite_off_amount')) {
            
                        return 'receipt_write_off_textarea_id';
                    }
            
                }
                return '';
            }
            
            function csh513_save_description() {
                var param = [];
                var ds_id = 'csh_transaction_receipt_write_off_result_ds';
                // var records = $(ds_id).getSelected();
                // if (records.length == 0) {
                // }
                var records = $(ds_id).getAll();
            
                for (var i = 0;i < records.length;i++) {
                    var record = records[i];
                    if (record.dirty) {
                        if (!record.get('description') && record.get('lease_channel') == '01') {
                            Aurora.showInfoMessage('提示', '摘要不能为空');
                            return false;
                        }
                        param.push({
                            'transaction_id': record.get('transaction_id'),
                            'lease_channel':record.get('lease_channel'),
                            description: record.get('description'),
                            '_status': 'update'
                        });
                    }
                }
                Aurora.request({
                    url: '${/request/@context_path}/autocrud/csh.CSH513.csh_update_description/batch_update',
                    para: param,
                    success: function() {
            
                        Aurora.SideBar.show({
                            msg: '保存成功',
                            duration: 2000
                        });
                        $(ds_id).query();
                    }
                });
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="currency_ds">
                <a:datas dataSource="/model/currency"/>
            </a:dataSet>
            <a:dataSet id="hls_lease_channel_ds" loadData="true" model="basic.hls_lease_channel_for_lov"/>
            <a:dataSet id="status_ds" lookupCode="CSH_PAYMENT_REQ_APPROVAL_STATUS"/>
            <a:dataSet id="csh_transaction_receipt_write_off_query_ds" autoCreate="true">
                <a:fields>
                    <a:field name="bp_type" lovGridHeight="350" lovHeight="500" lovService="basic.hls_bp_type_v_for_lov" lovWidth="500" title="HLS_BP_TYPE.BP_TYPE">
                        <a:mapping>
                            <a:map from="description" to="bp_type_desc"/>
                            <a:map from="bp_type" to="bp_type"/>
                            <a:map from="bp_category_desc" to="bp_category_desc"/>
                            <a:map from="bp_category" to="bp_category"/>
                            <a:map from="auto_coding" to="auto_coding"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="description"/>
                    <a:field name="bp_type_desc" readOnly="true"/>
                    <a:field name="bp_category" readOnly="true"/>
                    <a:field name="bp_category_desc" readOnly="true"/>
                    <a:field name="bp_code" lovGridHeight="350" lovHeight="500" lovService="basic.hls_bp_master_v_for_lov" lovWidth="500" title="HLS.BP_TITLE">
                        <a:mapping>
                            <a:map from="bp_id" to="bp_id"/>
                            <a:map from="bp_name" to="bp_name"/>
                            <a:map from="bp_code" to="bp_code"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bp_name" readOnly="true"/>
                    <a:field name="bp_id"/>
                    <a:field name="transaction_num_from"/>
                    <a:field name="transaction_num_to"/>
                    <a:field name="bank_slip_num_from"/>
                    <a:field name="bank_slip_num_to"/>
                    <a:field name="transaction_date_from" validator="dateValidator"/>
                    <a:field name="transaction_date_to" validator="dateValidator"/>
                    <a:field name="bank_account_code_from" autoCompleteField="bank_account_num" lovGridHeight="350" lovHeight="550" lovLabelWidth="90" lovService="csh.CSH101.csh_bank_account_lov" lovWidth="580" title="CSH511.BANK_ACCOUNT_CODE">
                        <a:mapping>
                            <a:map from="bank_account_code" to="bank_account_code_from"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bank_account_from_id"/>
                    <a:field name="bank_account_code_to" autoComplete="true" autoCompleteField="bank_account_num" lovGridHeight="350" lovHeight="550" lovLabelWidth="90" lovService="csh.CSH101.csh_bank_account_lov" lovWidth="580" title="CSH511.BANK_ACCOUNT_CODE">
                        <a:mapping>
                            <a:map from="bank_account_code" to="bank_account_code_to"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="transaction_amount_from"/>
                    <a:field name="transaction_amount_to"/>
                    <a:field name="posted_flag" defaultValue="Y"/>
                    <a:field name="currency_name_from_display" displayField="currency_name" options="currency_ds" returnField="currency_code_from" valueField="currency_code"/>
                    <a:field name="status_n" displayField="code_value_name" options="status_ds" returnField="status" valueField="code_value"/>
                    <a:field name="currency_code_from"/>
                    <a:field name="currency_name_to_display" displayField="currency_name" options="currency_ds" returnField="currency_code_to" valueField="currency_code"/>
                    <a:field name="currency_code_to"/>
                    <a:field name="receipt" checkedValue="RECEIPT" defaultValue="RECEIPT" uncheckedValue="N"/>
                    <a:field name="deposit" checkedValue="DEPOSIT" defaultValue="N" uncheckedValue="N"/>
                    <a:field name="risk" checkedValue="RISK" defaultValue="N" uncheckedValue="N"/>
                    <a:field name="advance_receipt" checkedValue="ADVANCE_RECEIPT" defaultValue="N" uncheckedValue="N"/>
                    <a:field name="transaction_category" defaultValue="BUSINESS"/>
                    <a:field name="full_write_off_flag" defaultValue="FULL"/>
                    <a:field name="reversed_flag" defaultValue="N"/>
                    <a:field name="full_returned_flag" defaultValue="FULL"/>
                    <a:field name="returned_flag"/>
                    <a:field name="bp_code_from" lovGridHeight="350" lovHeight="500" lovService="basic.hls_bp_master_v_for_lov" lovWidth="500" title="HLS.BP_TITLE">
                        <a:mapping>
                            <a:map from="bp_id" to="bp_id_from"/>
                            <a:map from="bp_name" to="bp_name_from"/>
                            <a:map from="bp_code" to="bp_code_from"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bp_code_to" lovGridHeight="350" lovHeight="500" lovService="basic.hls_bp_master_v_for_lov" lovWidth="500" title="HLS.BP_TITLE">
                        <a:mapping>
                            <a:map from="bp_id" to="bp_id_to"/>
                            <a:map from="bp_name" to="bp_name_to"/>
                            <a:map from="bp_code" to="bp_code_to"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="lease_channel"/>
                    <a:field name="lease_channel_desc" displayField="description" options="hls_lease_channel_ds" returnField="lease_channel" valueField="lease_channel"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="csh513_receipt_write_off_update"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="csh_transaction_receipt_write_off_result_ds" autoQuery="true" model="csh.CSH513.csh_transaction_query" queryDataSet="csh_transaction_receipt_write_off_query_ds" selectable="true" selectionModel="single">
                <a:fields>
                    <a:field name="auto_filt_penalty" checkedValue="Y" uncheckedValue="N"/>
                    <a:field name="auto_filt_prinicipal_interest" checkedValue="Y" uncheckedValue="N"/>
