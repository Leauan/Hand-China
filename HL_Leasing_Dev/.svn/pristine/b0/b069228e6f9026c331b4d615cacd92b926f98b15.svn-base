<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin
            		cus_con_gps_pkg.update_ast_zr_gps(
            		   p_contract_id 	 =>${@contract_id},
                       p_address     	 =>${@address},
                       p_savetype    	 =>${@savetype},
                       p_zr_id       	 =>${@zr_id},
                       p_zr_appcode      =>${@zr_appcode},
                       p_zr_locationtime =>to_date(${@zr_locationtime},'yyyy-mm-dd hh24:mi:ss'),
                       p_zr_orderstatus  =>${@zr_orderstatus},
                       p_zr_url          =>${@zr_url},
                       p_user_id     	 =>${/session/@user_id});
            	end;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
