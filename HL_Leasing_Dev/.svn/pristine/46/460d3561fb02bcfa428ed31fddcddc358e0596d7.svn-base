<bm:model xmlns:e="aurora.service.exception" xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                declare
                 ret varchar2(200);
        		begin
        		
				  ret := HLS_EBANK_PKG.auto_write_off;

				end;
        	]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:parameters>
                <bm:parameter name="ret" output="true"/>
                <bm:parameter name="error_msg" output="true"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
        		begin
        		
				  hls_bill99_request_pkg.nc_embk_parse_xml(p_response_xml  =>${@response_xml},
                              p_function_name =>${@function_name},
                              p_request_id   =>${@request_id},
                              p_interface_id  =>${@interface_id},
                              p_user_id      =>-1,
                              p_ret         =>${@ret},
                              p_error_msg    =>${@error_msg});

				end;
        	]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
