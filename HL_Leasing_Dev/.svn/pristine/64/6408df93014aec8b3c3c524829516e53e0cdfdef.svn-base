<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="batch_id" dataType="java.lang.Long" input="true" output="false" required="true"/>
                <bm:parameter name="file_name" dataType="java.lang.String" input="true" output="false" required="true"/>
                <bm:parameter name="file_size" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter name="file_path" dataType="java.lang.String" input="true" output="false" required="true"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    CREDIT_QUERY_PKG.CREDIT_ATACHMENT_REF
                    (
                        p_user_id=>${/session/@user_id},
                        p_batch_id=>${@batch_id},
                        p_file_name=>${@file_name},
                        p_file_size=>${@file_size},
                        p_file_path=>${@file_path}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
