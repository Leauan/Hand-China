<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    t.prj_contact_info_id,
                    t.prj_bp_id,
                    t.contact_person,
                    t.cell_phone,
                    t.comments,
                    t.contact_person_1,
                    t.cell_phone_1,
                    t.comments_1
                FROM
                    prj_project_bp_contact_info_lv t,
                    prj_project_bp_tenant_lv pb
                WHERE
                    t.prj_bp_id   = pb.prj_bp_id AND
                    pb.project_id = ${@project_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="prj_contact_info_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PRJ_CONTACT_INFO_ID" required="true"/>
        <bm:field name="prj_bp_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PRJ_BP_ID" required="true"/>
        <bm:field name="contact_person" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CONTACT_PERSON"/>
        <bm:field name="cell_phone" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CELL_PHONE"/>
        <bm:field name="comments" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="COMMENTS"/>
        <bm:field name="contact_person_1" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CONTACT_PERSON_1"/>
        <bm:field name="cell_phone_1" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CELL_PHONE_1"/>
        <bm:field name="comments_1" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="COMMENTS_1"/>
    </bm:fields>
</bm:model>
