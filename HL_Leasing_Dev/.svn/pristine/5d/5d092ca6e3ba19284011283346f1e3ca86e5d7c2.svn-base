                // var division = record.get('division');
                //var bp_id_tenant = record.get('bp_id_tenant');
                if (ds.id == '${/parameter/@layout_code}_F_BASIC_con_contract_ds') {
                    var bp_id_tenant = record.get('bp_id_tenant');
                    Aurora.request({
                        url: $('get_bp_class_link_id').getUrl(),
                        para: {
                            bp_id_tenant: bp_id_tenant
                        },
                        success: function(res) {
                            var bp_class_flag = res.result.bp_class_flag;
                            //alert(bp_class_flag);
                            if (bp_class_flag == 'NP') {
                                record.set('ebank_account_prop', '20');
                                record.set('ebank_account_prop_n', '个人账户');
                                record.getField('ebank_account_prop').setReadOnly(true);
                                record.getField('ebank_account_prop_n').setReadOnly(true);
                            }
                        },
                        failure: function() {
            
                           },
                        error: function() {
            
                           },
                        scope: this
                    });
            
                }
                if (ds.id == '${/parameter/@layout_code}_F_INVOICE_con_contract_lease_item_ds') {
                    //debugger;
                    var contract_ds_id = get_dsid_by_basetable(window['${/parameter/@layout_code}_layoutDataSetList'], 'con_contract');
                    var con_record = $(contract_ds_id).getAt(0);
                    if (con_record) {
                        var division = con_record.get('division');
                        if (division == '00') {
                            record.getField('bp_name_invoice').setRequired(true);
                            record.getField('invoice_amt').setRequired(true);
                            record.getField('invoice_num').setRequired(true);
                            record.getField('added_invoice_amt').setRequired(true);
                            record.getField('added_invoice_num').setRequired(true);
                            record.getField('trn_of_invoice').setRequired(true);
                            record.getField('invoice_date').setRequired(true);
                        }
                    }
                }
            };
