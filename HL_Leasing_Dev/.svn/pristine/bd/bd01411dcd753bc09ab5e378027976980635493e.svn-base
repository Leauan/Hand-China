<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="fasle">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    hls_bp_master_pkg.bp_bank_account_num( p_bank_account_num =>${@bank_account_num}, p_num =>${@num} );
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="num" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@num"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
