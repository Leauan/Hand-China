<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    a.code_value code ,
                    a.code_value_name NAME
                FROM
                    sys_code_values_v a
                WHERE
                    a.code                    = 'LIVING_SITUATION' AND
                    a.code_enabled_flag       = 'Y' AND
                    a.code_value_enabled_flag = 'Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CODE"/>
        <bm:field name="name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="NAME"/>
    </bm:fields>
</bm:model>
