<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:s="aurora.plugin.script" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="false">
    <a:init-procedure>
        <a:model-update model="csh.CSH602.yonda_hls_tl_check_table_authority"/>
        <s:server-script><![CDATA[
            importPackage(Packages.com.allinpay.xmltrans.yondatl);
            var yondatl_itfc = new YondaTl($instance('aurora.database.service.IDatabaseServiceFactory'), $instance('uncertain.ocm.IObjectRegistry'), $ctx.getData());
            var sync_bm = $bm('csh.CSH603.yonda_hls_tl_itfc_synchronization');
            var sub_tn_map = sync_bm.queryAsMap();
            sub_childs = sub_tn_map.getChildren();
            for (var i = 0;i < sub_childs.length;i++) {
                var sub_child = sub_childs[i];
                yondatl_itfc.queryRun(sub_child.tl_itfc_header_id, '200004');
            }
        ]]></s:server-script>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
