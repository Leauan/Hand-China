<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="CON_COLLECTION_METHODS" needAccessControl="false">
    <bm:fields>
        <bm:field name="methods_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="METHODS_ID" prompt="CON_COLLECTION_METHODS.METHODS_ID"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DESCRIPTION" prompt="CON_COLLECTION_METHODS.DESCRIPTION"/>
        <bm:field name="methods_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="METHODS_NAME" prompt="CON_COLLECTION_METHODS.METHODS_NAME"/>
        <bm:field name="enable_flag"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="methods_id"/>
    </bm:primary-key>
    <bm:data-filters>
        <bm:data-filter enforceOperations="query" expression="t1.enable_flag = &apos;Y&apos;"/>
    </bm:data-filters>
