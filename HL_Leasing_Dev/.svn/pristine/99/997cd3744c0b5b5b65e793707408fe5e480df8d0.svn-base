<?xml version="1.0" encoding="UTF-8"?>

<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select m.gps_sp_name, m.gps_sp_pwd
  from hls_bp_master m, con_contract_bp b
 where b.bp_id = m.bp_id
   and b.bp_category = m.bp_category
   and b.bp_category = 'AGENT'
   and b.contract_id =${@contract_id}]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="contract_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
