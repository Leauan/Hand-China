<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="prj_project_bp" needAccessControl="false">
    <bm:fields>
        <bm:field name="bp_name"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
select t5.bp_name
                             from prj_project_bp t5
                            where t5.project_id =  ${@project_id}
                              and t5.bp_category = 'TENANT'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
