<bm:model xmlns:o="aurora.database.local.oracle" xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="HLS_PRICE_LIST_LEVEL_DEFINE" defaultOrderBy="t1.seq asc">
    <bm:fields>
        <bm:field name="level_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="LEVEL_ID"/>
        <bm:field name="level_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="LEVEL_NAME"/>
        <bm:field name="price_list" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="PRICE_LIST"/>
        <bm:field name="parent_level_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="PARENT_LEVEL_ID"/>
        <bm:field name="seq" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SEQ"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG"/>
        <bm:field name="doc_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DOC_FLAG"/>
        <bm:field name="expand_flag" expression="&apos;Y&apos;" forInsert="false" forUpdate="false"/>
        <bm:field name="price_list_name" expression="(select h.description from hls_price_list h where h.price_list=t1.price_list)" forInsert="false" forUpdate="false"/>
    </bm:fields>
    <bm:primary-key>
        <bm:pk-field name="level_id"/>
    </bm:primary-key>
    <bm:features>
        <o:sequence-pk/>
        <f:standard-who/>
    </bm:features>
</bm:model>
