<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                 SELECT
                        t.hd_id,
                        t.bp_id,
                        bp_name
                    FROM hls_linkage_bp_hd t,hls_bp_master b
                    where t.bp_id =b.bp_id
                     and  t.hd_id =${@hd_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
