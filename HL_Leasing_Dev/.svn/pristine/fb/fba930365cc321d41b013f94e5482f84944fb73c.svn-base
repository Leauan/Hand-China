<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    nvl(t.sql_content,t.sql) sql_content
                FROM
                    hls_doc_file_templet_para t
                WHERE
                    t.bookmark = ${@bookmark}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="sql_content" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SQL"/>
    </bm:fields>
</bm:model>
