<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:parameters>
                <bm:parameter name="reverse_offset_id" dataType="java.lang.Long" output="true" outputPath="/parameter/@reverse_offset_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
            begin
            csh_offset_pkg.run_reversed_offset(p_reverse_offset_id=>${@reverse_offset_id},
                                p_offset_id=>${@offset_id},
                                p_reversed_date=>to_date(${@reverse_date},'yyyy-mm-dd'),
                                p_description=>${@description},
                                p_user_id=>${/session/@user_id});
            end;
]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
