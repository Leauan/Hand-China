<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:fields>
        <bm:field name="contract_lease_item_id"/>
        <bm:field name="item_frame_number"/>
        <bm:field name="item_engine_number"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    c3.contract_lease_item_id,
                    c3.item_frame_number,
                    c3.item_engine_number
                FROM
                    con_contract_item_detail c3 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="contract_lease_item_id" queryExpression="c3.contract_lease_item_id = ${@contract_lease_item_id}"/>
    </bm:query-fields>
</bm:model>
