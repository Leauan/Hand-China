<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure/>
    <a:view>
        <script><![CDATA[
            function back() {
                $('error_window').close();
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="error_result_ds" autoPageSize="true" autoQuery="true" queryUrl="${/request/@context_path}/autocrud/csh.CSH750.hl_transfer_fee_log/query?header_id=${/parameter/@header_id}">
                <a:fields>
                    <a:field name="message" readOnly="true"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:gridButton click="back" text="HLS.BACK"/>
            </a:screenTopToolbar>
            <a:form title="错误信息日志">
                <a:grid bindTarget="error_result_ds" height="200" navBar="true" width="450">
                    <a:columns>
                        <a:column name="message" align="center" prompt="错误信息" width="400"/>
                    </a:columns>
                </a:grid>
            </a:form>
        </a:screenBody>
    </a:view>
</a:screen>
