<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1">
    <bm:fields>
        <bm:field name="bp_category" prompt="类别编码"/>
        <bm:field name="description" prompt="类别名称"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
    			select * from (
    				SELECT bp_category,description FROM hls_bp_category WHERE enabled_flag = 'Y'
    				and (bp_category = 'BPO' or bp_category = 'AGENT')
    				) t1
    		]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
