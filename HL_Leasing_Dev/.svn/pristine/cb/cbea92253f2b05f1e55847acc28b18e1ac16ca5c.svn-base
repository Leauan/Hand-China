<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="rp_agreement_id" dataType="java.lang.Long" input="true" output="true" outputPath="/parameter/@rp_agreement_id"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                BEGIN
                    con_contract_rp_pkg.save_contract_rp_hd(p_rp_agreement_id  	=> ${@rp_agreement_id},
                    										p_contract_id		=> ${@contract_id},
                    										p_repurchase_date	=> to_date(${@repurchase_date},'yyyy-mm-dd'),
                    										p_rp_type			=> ${@rp_type},
                    										p_user_id			=> ${/session/@user_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
