<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="HLS_CCB_MANUAL_DOC_TMP">
    <bm:fields>
        <bm:field name="session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SESSION_ID" prompt="HLS_CCB_MANUAL_DOC_TMP.SESSION_ID"/>
        <bm:field name="document_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="DOCUMENT_ID" prompt="HLS_CCB_MANUAL_DOC_TMP.DOCUMENT_ID"/>
        <bm:field name="rec_amount" databaseType="NUMBER" datatype="java.lang.Long" physicalName="REC_AMOUNT" prompt="HLS_CCB_MANUAL_DOC_TMP.REC_AMOUNT"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                BEGIN
                    INSERT
                    INTO
                        hls_ccb_manual_doc_tmp
                        (
                            session_id,
                            document_id,
                            rec_amount
                        )
                        VALUES
                        (
                            ${/session/@session_id},
                            ${@document_id},
                            ${@manual_rec_amount}
                        );
                    COMMIT;
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="delete">
			<bm:update-sql><![CDATA[
         BEGIN
		    --update by 5390 采用session分批的方式，这里删session需要取整
        	delete hls_ccb_manual_doc_tmp where floor(session_id) = ${/session/@session_id};
			commit;
                END;
        ]]></bm:update-sql>
		</bm:operation>
    </bm:operations>
</bm:model>
