<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="CSH_DF_BATCH_TEMP_LOGS">
    <bm:fields>
        <bm:field name="csh_df_batch_temp_logs_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="CSH_DF_BATCH_TEMP_LOGS_ID" prompt="日志表主键"/>
        <bm:field name="batch_session_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="BATCH_SESSION_ID" prompt="SESSION_ID"/>
        <bm:field name="transaction_date" databaseType="DATE" datatype="java.util.Date" physicalName="TRANSACTION_DATE" prompt="收款日期"/>
        <bm:field name="transaction_amount" databaseType="NUMBER" datatype="java.lang.Double" physicalName="TRANSACTION_AMOUNT" prompt="收款金额"/>
        <bm:field name="bank_serial_num" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BANK_SERIAL_NUM" prompt="银行流水号"/>
        <bm:field name="bp_bank_account_num" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_BANK_ACCOUNT_NUM" prompt="账号(打款)"/>
        <bm:field name="bp_bank_account_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_BANK_ACCOUNT_NAME" prompt="账户名称(打款)"/>
        <bm:field name="bp_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BP_NAME" prompt="商业伙伴"/>
        <bm:field name="fee_type" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="FEE_TYPE" prompt="费用类型"/>
        <bm:field name="guangsan_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="GUANGSAN_FLAG" prompt="是否广三代付"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DESCRIPTION" prompt="摘要"/>
        <bm:field name="lease_channel" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="LEASE_CHANNEL" prompt="商业模式"/>
        <bm:field name="bank_account_num" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BANK_ACCOUNT_NUM" prompt="账号(收款)"/>
        <bm:field name="guangsan_flag_desc" expression="nvl((select d.code_value_name from sys_code_values_v d where code=&apos;YES_NO&apos; and d.code_value=t1.guangsan_flag), t1.ref_v03)" forInsert="false" forUpdate="false" prompt="是否广三代付"/>
        <bm:field name="lease_channel_desc" expression="(select d.description from hls_lease_channel d where d.lease_channel=t1.lease_channel)" forInsert="false" forUpdate="false" prompt="商业模式"/>
        <bm:field name="fee_type_desc" expression="nvl((select ci.description from hls_cashflow_item ci where ci.cf_item = t1.fee_type), t1.ref_v02)" forInsert="false" forUpdate="false" prompt="费用类型"/>
