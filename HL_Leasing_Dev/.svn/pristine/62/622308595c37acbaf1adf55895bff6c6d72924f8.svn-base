    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:query-fields>
        <bm:query-field field="project_id" queryOperator="="/>
        <bm:query-field field="contract_id" queryOperator="="/>
        <bm:query-field field="project_number" queryOperator="like"/>
        <bm:query-field field="contract_number" queryOperator="like"/>
        <bm:query-field name="company_authority" queryExpression="t1.company_id in (select company_id from fnd_companies start with company_id =${@company_authority} connect by prior company_id = parent_biz_company_id)"/>
    </bm:query-fields>
    <bm:data-filters>
        <bm:data-filter name="query" expression="exists  (select 1           from aut_trx_user_authorize a1, aut_owner_user_authorize a2          where a1.trx_category = &apos;PROJECT&apos;            and a1.trx_id = t1.project_id            and trunc(sysdate) between a1.start_date and                nvl(a1.end_date, trunc(sysdate))            and a1.user_id = a2.owner_user_id            and a1.trx_category = a2.trx_category            and a2.authorized_user_id = ${/session/@user_id}            and trunc(sysdate) between a2.start_date and                nvl(a2.end_date, trunc(sysdate)))    "/>
    </bm:data-filters>
