<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[SELECT l.direct_debit_bank_id,
       l.direct_debit_bank_id_n,
       l.withhold_way,
       l.withhold_way_n,
       l.ebank_id_n,
       l.ebank_city,
       l.ebank_city_n,
       l.ebank_province,
       l.ebank_province_n,
       l.contract_id
  FROM con_contract_lv l where l.contract_id=${@contract_id}]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="direct_debit_bank_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="DIRECT_DEBIT_BANK_ID"/>
        <bm:field name="direct_debit_bank_id_n" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DIRECT_DEBIT_BANK_ID_N"/>
        <bm:field name="withhold_way" databaseType="VARCHAR2" datatype="java.lang.String"/>
        <bm:field name="withhold_way_n" databaseType="VARCHAR2" datatype="java.lang.String"/>
        <bm:field name="ebank_id_n" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_ID_N"/>
        <bm:field name="ebank_city" databaseType="NUMBER" datatype="java.lang.Long" physicalName="EBANK_CITY"/>
        <bm:field name="ebank_city_n" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_CITY_N"/>
        <bm:field name="ebank_province" databaseType="NUMBER" datatype="java.lang.Long" physicalName="EBANK_PROVINCE"/>
        <bm:field name="ebank_province_n" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_PROVINCE_N"/>
        <bm:field name="contract_id" databaseType="NUMBER" datatype="java.lang.Long"/>
    </bm:fields>
</bm:model>
