<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    con_contract_pkg.con_contract_check_bp_name_sp( p_contract_id => ${@contract_id}, p_name => ${@name}, p_user_id => ${/session/@user_id}, p_flag => ${@flag});
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="flag" dataType="java.lang.Long" input="false" output="true" outputPath="@flag"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
