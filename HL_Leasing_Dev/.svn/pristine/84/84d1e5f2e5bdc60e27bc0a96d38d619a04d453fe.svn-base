<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:o="aurora.database.local.oracle" xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="t1" baseTable="CSH_EBANK">
    <bm:fields>
        <bm:field name="ebank_id" databaseType="NUMBER" insertExpression="csh_bank_s.nextval" datatype="java.lang.Long" physicalName="EBANK_ID" prompt="CSH_EBANK.EBANK_ID"/>
        <bm:field name="ebank_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_CODE" prompt="CSH_EBANK.EBANK_CODE"/>
        <bm:field name="ebank_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_NAME" prompt="CSH_EBANK.EBANK_NAME"/>
        <bm:field name="ebank_province_city" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_PROVINCE_CITY" prompt="CSH_EBANK.EBANK_PROVINCE_CITY"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG" prompt="CSH_EBANK.ENABLED_FLAG"/>
        <bm:field name="enabled_flag_name" forInsert="false" forUpdate="false"/>
        <bm:field name="ebank_type_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="EBANK_TYPE_ID"/>
        <bm:field name="ebank_type_n" expression="(select             t.bank_short_name       from csh_bank_v t where t.bank_id = t1.ebank_type_id)" forInsert="false" forUpdate="false"/>
        <bm:field name="ebank_spd_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="EBANK_SPD_FLAG"/>
        <bm:field name="withhold_way" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="WITHHOLD_WAY" prompt="代扣方式"/>
        <bm:field name="withhold_way_name" forInsert="false" forUpdate="false"/>
        <bm:field name="cup_channel" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CUP_CHANNEL" prompt="银联扣款渠道"/>
        <bm:field name="cup_channel_name" forInsert="false" forUpdate="false"/>
        <bm:field name="seq_num" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SEQ_NUM" prompt="序号"/>
        <bm:field name="cup_pin" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="CUP_PIN" prompt="银联PIN码"/>
		<bm:field name="trx_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TRX_CODE" prompt="商户签约申请接口"/>
        <bm:field name="trx_code_name" forInsert="false" forUpdate="false"/>
		<bm:field name="limit_period_unit" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="LIMIT_PERIOD_UNIT" prompt="委托代扣限制周期"/>
        <bm:field name="limit_period_unit_name" forInsert="false" forUpdate="false"/>
		<bm:field name="max_cnt_limit" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="MAX_CNT_LIMIT" prompt="委托代扣限制周期内最大代扣次数"/>
		<bm:field name="single_limit" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SINGLE_LIMIT" prompt="委托代扣单笔最大金额"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
               SELECT t1.ebank_id,
       t1.ebank_code,
       t1.ebank_name,
       t1.ebank_province_city,
       t1.enabled_flag,
       (SELECT
                        v.code_value_name
                    FROM
                        sys_code_values_v v
                    WHERE
                        v.code       = 'ENABLE_FLAG' AND
                        v.code_value = t1.cup_channel
                    )   enabled_flag_name,
       t1.ebank_type_id,
       (SELECT t.bank_short_name
          FROM csh_bank_v t
         WHERE t.bank_id = t1.ebank_type_id) AS ebank_type_n,
       t1.ebank_spd_flag,
       t1.withhold_way,
       (SELECT
                        v.code_value_name
                    FROM
                        sys_code_values_v v
                    WHERE
                        v.code       = 'WITHHOLD_WAYS' AND
                        v.code_value = t1.withhold_way
                    )   withhold_way_name,
       t1.cup_channel,
       (SELECT
                        v.code_value_name
                    FROM
                        sys_code_values_v v
                    WHERE
                        v.code       = 'CUP_CHANNELS' AND
                        v.code_value = t1.cup_channel
                    )   cup_channel_name,
		 t1.trx_code,
       (SELECT
                        v.code_value_name
                    FROM
                        sys_code_values_v v
                    WHERE
                        v.code       = 'TRX_CODE' AND
                        v.code_value = t1.trx_code
                    )   trx_code_name,
		 t1.limit_period_unit,
       (SELECT
                        v.code_value_name
                    FROM
                        sys_code_values_v v
                    WHERE
                        v.code       = 'LIMIT_PERIOD_UNIT' AND
                        v.code_value = t1.limit_period_unit
                    )   limit_period_unit_name,
        t1.max_cnt_limit,
        t1.single_limit,
        t1.seq_num,
       t1.cup_pin
  FROM csh_ebank t1 #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="ebank_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field field="ebank_name" queryExpression="t1.ebank_name like &apos;%&apos; || ${@ebank_name} || &apos;%&apos;"/>
        <bm:query-field field="withhold_way" queryOperator="="/>
        <bm:query-field field="cup_channel" queryOperator="="/>
        <bm:query-field field="enabled_flag" queryOperator="="/>
    </bm:query-fields>
</bm:model>
