<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
				begin
					hl_gmmc_interface_pkg.finance_quota(p_bp_credit_hd_id     => ${@bp_credit_hd_id},
                          p_financing_org_code  => ${@financing_org_code},
                          p_host_plant_number   => ${@host_plant_number},
                          p_manufacturer_code   => ${@manufacturer_code},
                          p_gac_bp_code         => ${@gac_bp_code},
                          p_bp_name             => ${@bp_name},
                          p_credit_total_amount => ${@credit_total_amount},
                          p_used_amount         => ${@used_amount},
                          p_last_amount         => ${@last_amount},
                          p_withholding_amount  => ${@withholding_amount},
                          p_quota_status        => ${@quota_status});
				end;
			]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="bp_credit_hd_id" output="true" outputPath="@bp_credit_hd_id"/>
                <bm:parameter name="financing_org_code" output="true" outputPath="@financing_org_code"/>
                <bm:parameter name="host_plant_number" output="true" outputPath="@host_plant_number"/>
                <bm:parameter name="manufacturer_code" output="true" outputPath="@manufacturer_code"/>
                <bm:parameter name="gac_bp_code" output="true" outputPath="@gac_bp_code"/>
                <bm:parameter name="bp_name" output="true" outputPath="@bp_name"/>
                <bm:parameter name="credit_total_amount" output="true" outputPath="@credit_total_amount"/>
                <bm:parameter name="used_amount" output="true" outputPath="@used_amount"/>
                <bm:parameter name="last_amount" output="true" outputPath="@last_amount"/>
                <bm:parameter name="withholding_amount" output="true" outputPath="@withholding_amount"/>
                <bm:parameter name="quota_status" output="true" outputPath="@quota_status"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
