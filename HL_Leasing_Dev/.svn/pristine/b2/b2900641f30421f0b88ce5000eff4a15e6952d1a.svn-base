<bm:model xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="PRJ_PROJECT_DIRECTOR_APPROVER" needAccessControl="false">
    <bm:fields>
        <bm:field name="director_approver_record_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="DIRECTOR_APPROVER_RECORD_ID" prompt="PRJ_PROJECT_DIRECTOR_APPROVER.DIRECTOR_APPROVER_RECORD_ID"/>
        <bm:field name="approval_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="APPROVAL_ID" prompt="PRJ_PROJECT_DIRECTOR_APPROVER.APPROVAL_ID"/>
        <bm:field name="approver_user_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="APPROVER_USER_ID" prompt="PRJ_PROJECT_DIRECTOR_APPROVER.APPROVER_USER_ID"/>
        <bm:field name="director_comment" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DIRECTOR_COMMENT" prompt="PRJ_PROJECT_DIRECTOR_APPROVER.DIRECTOR_COMMENT"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk sequenceName="director_approver_s"/>
    </bm:features>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
    			select pda.director_approver_record_id,
       pda.approval_id,
       pda.approver_user_id,
       pda.director_comment
  from prj_project_director_approver pda
  where pda.approval_id= ${@approval_id}
    		]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:primary-key>
        <bm:pk-field name="director_approver_record_id"/>
    </bm:primary-key>
</bm:model>
