<?xml version="1.0" encoding="UTF-8"?>
<ns1:model xmlns:ns2="aurora.database.local.oracle" xmlns:ns1="http://www.aurora-framework.org/schema/bm">
    <ns1:operations>
        <ns1:operation name="update">
            <ns1:update-sql><![CDATA[
                begin
					hls_sap_accounts_attr_pkg.sap_accounts_attr_save(
						      p_account_set_id => ${@account_set_id},
                              p_account_id => ${@account_id},
                              p_sap_account_type => ${@sap_account_type},
                              p_dr_account_category => ${@dr_account_category},
                              p_cr_account_category => ${@cr_account_category},
                              p_gl_identity => ${@gl_identity},
                              p_cash_flow_flag => ${@cash_flow_flag},
                              p_sap_account_group => ${@sap_account_group},
                              p_trade_partner_flag => ${@trade_partner_flag},
                              p_user_id => ${/session/@user_id});                                          
                end;   
            ]]></ns1:update-sql>
        </ns1:operation>
    </ns1:operations>
</ns1:model>
