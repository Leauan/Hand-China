<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:fields>
        <bm:field name="code_value"/>
        <bm:field name="code_value_name"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    internal_period_num code_value,
                    period_name code_value_name
                FROM
                    gld_period_all_v
                WHERE
                    adjustment_flag='N' AND
                    start_date BETWEEN to_date('2015-01-01','YYYY-MM-DD') AND add_months(sysdate,15)
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
