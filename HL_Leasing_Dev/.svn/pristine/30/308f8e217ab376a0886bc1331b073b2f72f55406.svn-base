<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select h.payment_req_id,   
            r.create_date_fmt,    
            r.action_type_desc,
            r.comment_text,
            r.node_desc,
            r.approver 
            from csh_payment_req_hd h,
            zj_wfl_approve_history_v r
            where h.wfl_instance_id=r.instance_id
             and to_char(h.payment_req_id) in (${:@payment_req_id}) order by r.create_date_fmt
  ]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="payment_req_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
