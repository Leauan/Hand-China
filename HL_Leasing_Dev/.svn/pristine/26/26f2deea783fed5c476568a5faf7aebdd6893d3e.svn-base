        <bm:field name="docu_number" databaseType="VARCHAR2" datatype="java.lang.String" forInsert="false" forUpdate="false" physicalName="DOCU_NUMBER"/>
        <bm:field name="doc_type_n" databaseType="VARCHAR2" datatype="java.lang.String" forInsert="false" forUpdate="false" physicalName="DOC_TYPE_N"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    IF ${@judge_lend}='Y' THEN
                        INSERT
                        INTO
                            LEND_CONTENT_LIST
                            (
                                LEND_CONTENT_LIST_ID,
                                LEND_HEADER_ID,
                                CDD_ITEM_ID,
                                CONTRACT_ID,
                                CREATED_BY,
                                CREATION_DATE,
                                LAST_UPDATED_BY,
                                LAST_UPDATE_DATE
                            )
                            VALUES
                            (
                                lend_content_list_s.NEXTVAL,
                                ${../../@lend_header_id},
                                ${@cdd_item_id},
                                ${@contract_id},
                                ${/session/@user_id},
                                sysdate,
                                ${/session/@user_id},
                                sysdate
                            );
                    END IF;
                    
                    UPDATE
                        prj_cdd_item p
                    SET
                        LEND_FLAG= ${@lend_flag}
                    WHERE
                        p.cdd_item_id=${@cdd_item_id} ;
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
            	BEGIN
            	  contract_read_workflow_strat.contract_workflow_start(p_lend_header_id =>${@lend_header_id},
                                                       p_user_id => ${/session/@user_id});
            	END;
        	]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:features>
        <f:standard-who/>
        <o:sequence-pk sequenceName="yonda_document_lend_ln_s"/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="lend_line_id"/>
    </bm:primary-key>
    <bm:query-fields>
