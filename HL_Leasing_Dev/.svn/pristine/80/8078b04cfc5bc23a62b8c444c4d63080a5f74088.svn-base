<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    li.brand_id_n,--品牌
                    (li.model_id_n
                    ||li.series_id_n) ms_info,--车型车系
                    li.color_of_apprearance,  --颜色
                    li.item_frame_number,     --车架号,
                    li.item_engine_number,    --发动机号
                    li.license_number
                FROM
                    con_contract_lease_item_lv li
                WHERE
                    li.contract_id = ${@contract_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="brand_id_n"/>
        <bm:field name="ms_info"/>
        <bm:field name="color_of_apprearance"/>
        <bm:field name="item_frame_number"/>
        <bm:field name="item_engine_number"/>
        <bm:field name="license_number"/>
    </bm:fields>
</bm:model>
