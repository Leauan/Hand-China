<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:update-sql><![CDATA[
select h.*,
       hlo.description organization_desc,     
       e.DESCRIPTION org_desc
  from hls_price_list_conds_lease h,
       exp_org_unit_vl            e,
       hls_lease_organization     hlo
 where h.lease_organization = hlo.lease_organization
   and h.lease_org_unit = e.UNIT_CODE
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
