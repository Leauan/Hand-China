<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:update-sql><![CDATA[
                SELECT
                    hbm.bp_id value_code,
                    hbm.bp_name value_name,
                    hbm.bp_code
                FROM
                    hls_bp_master hbm
                WHERE
                    hbm.enabled_flag = 'Y' AND
                    hbm.bp_category  = 'AGENT'
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
