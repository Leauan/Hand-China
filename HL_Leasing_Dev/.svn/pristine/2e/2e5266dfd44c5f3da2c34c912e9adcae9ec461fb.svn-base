<a:service xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure>
        <s:server-script><![CDATA[
            importPackage(java.io);
            importPackage(com.test);
            try {
                var xml_temp;
                var deal_result;
                var parameter = $ctx.parameter;
                var manual_pub = $bm('db.hls_ebank_cup_pub_pkg.query_manual_pub_transaction');
                var manual_pub_result = manual_pub.execute({
                    "transaction_id": parameter.transaction_id,
                    "xml_temp": xml_temp
                });
                var hlCupManual = new HlCupManual();
                var result = hlCupManual.TestPayReq(parameter.xml_temp);
            
                var deal_transaction = $bm('db.hls_ebank_cup_pub_pkg.deal_query_transaction_result');
                deal_transaction.execute({
                    "transaction_id": parameter.transaction_id,
                    "xml_clob": result,
                    "xml_clob_send": parameter.xml_temp,
                    "deal_result": deal_result
                });
            } catch (e) {
                println("错误信息" + e);
            }
        ]]></s:server-script>
    </a:init-procedure>
</a:service>
