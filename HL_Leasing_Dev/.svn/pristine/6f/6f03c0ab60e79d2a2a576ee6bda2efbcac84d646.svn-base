<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[SELECT T1.* FROM (SELECT * FROM hls_bp_master_np_v a WHERE a.bp_category = 'TENANT_SEC') T1 #WHERE_CLAUSE#]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="id_card_no" queryExpression="t1.id_card_no=${@id_card_no} and rownum&lt;2"/>
        <bm:query-field name="id_type" queryExpression="t1.id_type=${@id_type}"/>
    </bm:query-fields>
</bm:model>
