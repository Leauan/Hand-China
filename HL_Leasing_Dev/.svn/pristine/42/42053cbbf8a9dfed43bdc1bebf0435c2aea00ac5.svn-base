<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    sv.code_value,
                    sv.code_value_name
                FROM
                    SYS_CODE_VALUES_V sv
                WHERE
                    code = 'AST_LICENSE_TYPE'
 and sv.code_enabled_flag = 'Y'
                and sv.code_value_enabled_flag = 'Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
