<a:service xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure>
        <s:server-script><![CDATA[
            var _status = $ctx.parameter._status;
            var hd_bm = $bm('cont.CON556.yonda_document_lend_record_hd');
            hd_bm.option = {
                updatePassedFieldOnly: 'true'
            }
            hd_bm[_status]();
        ]]></s:server-script>
        <a:batch-apply sourcepath="/parameter/line_records">
            <p:switch test="@current_parameter/@_status">
                <p:case value="insert">
                    <a:model-insert model="cont.CON556.yonda_document_lend_record_ln"/>
                </p:case>
                <p:case value="update">
                    <a:model-update model="cont.CON556.yonda_document_lend_record_ln"/>
                </p:case>
            </p:switch>
        </a:batch-apply>
        <a:batch-apply sourcepath="/parameter/cdd_records">
            <p:switch test="@current_parameter/@_status">
                <p:case value="insert">
                    <a:model-insert model="cont.CON556.yonda_document_lend_cdd"/>
                </p:case>
                <p:case value="update">
                    <a:model-update model="cont.CON556.yonda_document_lend_cdd"/>
                </p:case>
            </p:switch>
        </a:batch-apply>
        <!--   <a:model-execute model="cont.CON556.yonda_document_lend_record_hd"/> -->
    </a:init-procedure>
