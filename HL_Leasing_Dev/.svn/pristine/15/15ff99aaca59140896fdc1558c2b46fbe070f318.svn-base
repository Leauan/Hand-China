<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:p="uncertain.proc" xmlns:a="http://www.aurora-framework.org/application" xmlns:s="aurora.plugin.script" trace="true">
    <a:init-procedure>
        <s:server-script><![CDATA[
        	var m=$ctx.parameter.toXML();
        	println(m);
        ]]></s:server-script>
        <a:batch-apply sourcepath="/parameter">
            <p:switch test="@current_parameter/@_status">
                <p:case value="update">
                    <a:model-update model="cont.CON555A.con_express_hd"/>
                    <a:model-batch-update model="cont.CON555A.con_express_ln" sourcePath="@current_parameter/result_ds"/>
                </p:case>
                <p:case value="insert">
                    <a:model-insert model="cont.CON555A.con_express_hd"/>
                    <a:model-batch-update model="cont.CON555A.con_express_ln" sourcePath="@current_parameter/result_ds"/>
                </p:case>
                <p:case value="delete">
                    <a:model-delete model="cont.CON555A.con_express_hd"/>
                    <a:model-delete model="cont.CON555A.con_express_ln"/>
                </p:case>
            </p:switch>
        </a:batch-apply>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
