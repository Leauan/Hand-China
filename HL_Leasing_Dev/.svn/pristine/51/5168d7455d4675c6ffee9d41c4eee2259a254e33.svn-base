<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="xml_clob" dataType="java.sql.Clob" input="true" output="false"/>
                <bm:parameter name="transaction_id" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter name="seqno" dataType="java.lang.String" input="true" output="false"/>
                <bm:parameter name="deal_result" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@deal_result"/>
                <bm:parameter name="user_id" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter name="session_id" dataType="java.lang.Double" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_EBANK_CUP_PUB_PKG.DEAL_CREATE_TRANSACTION_RESULT
                    (
                        p_xml_clob=>${@xml_clob},
                        p_transaction_id=>${@transaction_id},
                        p_seqno=>${@seqno},
                        p_deal_result=>${@deal_result},
                        p_session_id=>${@session_id},
                        p_user_id=>${@user_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
