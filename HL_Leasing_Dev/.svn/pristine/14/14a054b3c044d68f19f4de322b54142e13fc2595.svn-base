<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" baseTable="csh_payment_req_hd">
    <bm:fields>
        <bm:field name="user_id"/>
        <bm:field name="user_name" displayWidth="200" forDisplay="true" forQuery="true" prompt="员工帐号"/>
        <bm:field name="employee_name" displayWidth="200" forDisplay="true" forQuery="true" prompt="员工姓名"/>
        <bm:field name="employee_code" displayWidth="150" forDisplay="true" forQuery="true" prompt="员工编码"/>
    </bm:fields>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                select * from zj_wfl_users_v t1
                #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="user_name" queryExpression="t1.user_name like &apos;%&apos; || upper(${@user_name}) || &apos;%&apos;"/>
        <bm:query-field name="employee_name" queryExpression="t1.employee_name like &apos;%&apos; || ${@employee_name} || &apos;%&apos;"/>
        <bm:query-field name="employee_code" queryExpression="t1.employee_code like &apos;%&apos; || ${@employee_code} || &apos;%&apos;"/>
    </bm:query-fields>
</bm:model>
