<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="return_value" input="false" output="true"/>
                <bm:parameter name="company_id" input="true" output="false"/>
                <bm:parameter name="acctno" input="true" output="false"/>
                <bm:parameter name="projectname" input="true" output="false"/>
                <bm:parameter name="projectnumber" input="true" output="false"/>
                <bm:parameter name="feeno" input="true" output="false"/>
                <bm:parameter name="payment_flag" input="true" output="false"/>
                <bm:parameter name="receipt_flag" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    ${@return_value} := RD_EBANK_SPD_PUB_PKG.SPD_TRANSACTION_TXDS70
                    (
                        p_company_id => ${/session/@company_id},
                        p_acctno=>${@acctno},
                        p_projectname=>${@projectname},
                        p_projectnumber=>${@projectnumber},
                        p_feeno=>${@feeno},
                        p_payment_flag=>${@payment_flag},
                        p_receipt_flag=>${@receipt_flag}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
