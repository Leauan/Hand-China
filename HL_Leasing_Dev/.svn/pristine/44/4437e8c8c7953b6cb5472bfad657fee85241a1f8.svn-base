<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    csh_payment_use_linkage_pkg.check_residual_amount(        p_payment_req_id         => ${@payment_req_id},
                                                                              p_project_number         => ${@project_number},
                                                                              p_bp_id_tenant           => ${@bp_id_tenant},
                                                                              p_check_result           => ${@check_result},
                                                                              p_agent_user_id          => ${@agent_user_id});
                END;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="check_result" dataType="java.lang.String" output="true" outputPath="@check_result"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                 BEGIN
                    csh_payment_use_linkage_pkg.whether_pick_all(           p_whether_pick_all       =>${@whether_pick_all},
                                                                            p_payment_req_number     => ${@payment_req_number});
                END;
            ]]></bm:update-sql>
        </bm:operation>
