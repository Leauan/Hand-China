<?xml version="1.0" encoding="UTF-8"?>
<a:service xmlns:s="aurora.plugin.script" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure>
        <s:server-script import="yonda_itfc_config/tl_withholding_config.js"><![CDATA[
            importPackage(Packages.com.allinpay.xmltrans.yondatl);
            var yondatl_itfc = new YondaTl($instance('aurora.database.service.IDatabaseServiceFactory'), $instance('uncertain.ocm.IObjectRegistry'), $ctx.getData());
            var merchantid = tl_withholding_config['merchantId'];
            var root = $ctx.parameter;
            var children = root.getChildren();
            var single_daishou_bm = $bm('csh.CSH602.yonda_hls_tl_single_daishou');
            for (var i = 0;i < children.length;i++) {
                var child = children[i];
                child.merchantid = merchantid;
                single_daishou_bm.update(child);
                yondatl_itfc.run(child.tl_itfc_header_id, '100011');
            }
        ]]></s:server-script>
    </a:init-procedure>
    <a:service-output output="/parameter"/>
</a:service>
