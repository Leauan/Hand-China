<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    hls_bp_master_np_v a
                WHERE
                    a.id_card_no   = ${@id_card_no} AND
                    a.id_type      ='ID_CARD' AND
                    a.enabled_flag = 'Y' AND
                    rownum         = 1
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
