<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    cc.contract_id,
                    cc.contract_number,
                    cc.bp_id_tenant,
                    cc.contract_file_date,
                    cc.project_number,
                    cc.bp_name,
                    cc.business_type_desc,
                    cc.division_desc,
                    cc.bp_class_desc,
                    cc.invoice_agent_desc,
                    cc.project_id,
                    cc.full_write_off_date
                FROM
                    CON_CONTRACT_V cc
                WHERE
                    cc.contract_id = ${/parameter/@contract_id}
            ]]></bm:query-sql>
            <bm:parameters>
                <bm:parameter inputPath="/parameter/@contract_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
