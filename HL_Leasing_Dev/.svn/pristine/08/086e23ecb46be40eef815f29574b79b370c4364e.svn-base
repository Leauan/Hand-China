<?xml version="1.0" encoding="UTF-8"?>
<!--
	&author:DJ
	$date:2013/11/20
	$purpose:项目上会工作流评委意见汇总
-->
<a:screen xmlns:c="aurora.application.action" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure><![CDATA[
    ]]></a:init-procedure>
    <a:view>
        <script><![CDATA[
        ]]></script>
        <a:switch test="/parameter/@show_project_info">
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_document_project_info.screen?project_id=${/parameter/@project_id}&amp;show_project_survey_print=${/parameter/@show_project_survey_print}&amp; show_project_view_print=${/parameter/@show_project_view_print}&amp;show_project_credit_check_print=${/parameter/@show_project_credit_check_print}&amp;show_project_opinion_vote_print=${/parameter/@show_project_opinion_vote_print}&amp;show_approver_table_print=${/parameter/@show_approver_table_print}&amp;approval_id=${/parameter/@approval_id}&amp;prj_wfl_calc_hd_id=${/parameter/@prj_wfl_calc_hd_id}"/>
            </a:case>
            <a:case value="W">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_document_project_info.screen?project_id=${/parameter/@project_id}&amp;show_project_survey_print=${/parameter/@show_project_survey_print}&amp; show_project_view_print=${/parameter/@show_project_view_print}&amp;show_project_credit_check_print=${/parameter/@show_project_credit_check_print}&amp;show_project_opinion_vote_print=${/parameter/@show_project_opinion_vote_print}&amp;show_project_info=${/parameter/@show_project_info}&amp;show_approver_table_print=${/parameter/@show_approver_table_print}&amp;approval_id=${/parameter/@approval_id}&amp;prj_wfl_calc_hd_id=${/parameter/@prj_wfl_calc_hd_id}&amp;prj_wfl_calc_hd_id=${/parameter/@prj_wfl_calc_hd_id}"/>
            </a:case>
        </a:switch>
        <a:switch test="/parameter/@show_project_and_up_down">
            <a:case value="Y">
                <a:screen-include screen="modules/prj/PRJ501/prj_project_detail_and_updown.screen?project_id=${/parameter/@project_id}&amp;cdd_item=${/parameter/@cdd_item}&amp;prj_wfl_calc_hd_id=${/parameter/@prj_wfl_calc_hd_id}"/>
            </a:case>
        </a:switch>
        <a:switch test="/parameter/@show_approver">
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approver_comment_list.screen?approval_id=${/parameter/@approval_id}"/>
            </a:case>
        </a:switch>
        <a:switch test="/parameter/@show_summary">
            <a:case value="W">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approval_comment_summary.screen?approval_id=${/parameter/@approval_id}&amp;readOnly=false&amp;show_save=Y"/>
            </a:case>
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approval_comment_summary.screen?approval_id=${/parameter/@approval_id}&amp;readOnly=true&amp;show_save=N"/>
            </a:case>
        </a:switch>
        <a:switch test="/parameter/@show_reply">
            <a:case value="W">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approval_project_manager_reply.screen?approval_id=${/parameter/@approval_id}&amp;readOnly=false&amp;show_save=Y&amp;show_reply_attachment=${/parameter/@show_reply_attachment}"/>
            </a:case>
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approval_project_manager_reply.screen?approval_id=${/parameter/@approval_id}&amp;readOnly=true&amp;show_save=N&amp;show_reply_attachment=${/parameter/@show_reply_attachment}"/>
            </a:case>
        </a:switch>
        <a:switch test="/parameter/@show_approver_choose">
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_approver_create.screen?approval_id=${/parameter/@approval_id}&amp;approver_choose_edit_flag=${/parameter/@approver_choose_edit_flag}"/>
            </a:case>
        </a:switch>
    </a:view>
</a:screen>
