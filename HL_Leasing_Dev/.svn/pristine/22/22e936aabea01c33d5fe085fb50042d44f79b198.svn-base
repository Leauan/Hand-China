<a:screen xmlns:a="http://www.aurora-framework.org/application" trace="true">
    <a:init-procedure><![CDATA[
    ]]></a:init-procedure>
    <a:view>
        <script><![CDATA[
            function hls360_nc_bp_close() {
               $('hls_nc_bp_log_window').close();
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="hls_nc_bp_log_query" autoPageSize="true" autoQuery="true" model="nc.NC100.hls_nc_bp_log" queryUrl="${/request/@context_path}/autocrud/nc.NC100.hls_nc_bp_log/query?result_id=${/parameter/@result_id}">
                <a:fields>
                    <a:field name="result_errormasg" readOnly="true"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
        <a:screenBody>
            <a:screenTopToolbar>
                <a:toolbarButton click="hls360_nc_bp_close" text="HLS.CLOSE"/>
            </a:screenTopToolbar>
            <a:form column="1" labelSeparator=" " title="日志查看">
                <a:textArea name="result_errormasg" bindTarget="hls_nc_bp_log_query" prompt="日志明细" style="width:600px;height:280px"/>
            </a:form>
        </a:screenBody>
    </a:view>
</a:screen>
