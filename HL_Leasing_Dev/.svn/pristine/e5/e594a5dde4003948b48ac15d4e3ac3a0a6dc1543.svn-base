<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
            	begin
            		con_change_req_extension_pkg.bm_check_after_save(
            			p_change_req_id   =>${@change_req_id},
                        p_user_id         =>${/session/@user_id},
                        p_change_req_number =>${@change_req_number}
                     );
            	end;
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="change_req_number" dataType="java.lang.String" input="false" output="true" outputPath="@change_req_number"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
