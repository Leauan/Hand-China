<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[delete from HL_CONTENT_BATCH_DOWN_TMP 
where session_id = ${/session/@session_id}]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter inputPath="/session/@session_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
        		
        			insert into HL_CONTENT_BATCH_DOWN_TMP(content_id,session_id) values(${@content_id},${/session/@session_id})
        		
        	]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
