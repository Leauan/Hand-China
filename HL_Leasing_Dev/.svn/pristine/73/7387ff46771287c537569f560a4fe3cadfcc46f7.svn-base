<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="xml_temp" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@xml_temp"/>
                <bm:parameter name="transaction_id" dataType="java.lang.Long" input="false" output="true" outputPath="/parameter/@transaction_id"/>
                <bm:parameter name="seqno" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@seqno"/>
                <bm:parameter name="user_id" dataType="java.lang.Long" input="true" output="false"/>
                <bm:parameter name="session_id" dataType="java.lang.Double" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_EBANK_CUP_PUB_PKG.GET_MANUAL_PUB_TRANSACTION
                    (
                        p_xml_temp			=>${@xml_temp},
                        p_transaction_id 	=>${@transaction_id},
                        p_seqno        	 	=>${@seqno},
                        p_user_id			=>${@user_id},
                        p_session_id		=>${@session_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
