<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:parameters>
                <bm:parameter name="xml_temp" dataType="java.lang.String" input="false" output="true" outputPath="/parameter/@xml_temp"/>
                <bm:parameter name="note_verify" dataType="java.lang.String" input="true" output="false"/>
				<bm:parameter name="direct_debit_bank_id" dataType="java.lang.Long" input="true" output="false"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                begin
                    HLS_EBANK_CUP_PUB_PKG.HL_CUP_SIGN_CONFIRM
                    (
                        p_xml_temp=>${@xml_temp},
                        p_note_verify=>${@note_verify},
						p_direct_debit_bank_id=>${@direct_debit_bank_id},
                        p_user_id=>${/session/@user_id},
                        p_session_id=>${/session/@session_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
