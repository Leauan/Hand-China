<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select 
  count(*) v_count
from hls_lease_products lp
where lp.lease_products_code = ${@item_frame_number}
and lp.enable_flag = 'Y']]></bm:query-sql>
            <bm:parameters>
                <bm:parameter name="item_frame_number"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="v_count" databaseType="NUMBER" datatype="java.lang.Long" physicalName="v_count"/>
    </bm:fields>
</bm:model>
