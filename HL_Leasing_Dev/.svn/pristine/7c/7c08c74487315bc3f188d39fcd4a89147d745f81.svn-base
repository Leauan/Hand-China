<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                UPDATE
                    con_contract_add_apply t
                SET
                    t.apply_status='REJECTED'
                WHERE
                    t.apply_id=${@apply_id}
            
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="apply_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
